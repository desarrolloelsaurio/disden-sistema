FUNCTION CTRL2_OR
DO CASE
   CASE ULPR="SER"
     SET ORDER TO TAG IIF(TYPE("ORDSER")#'L' .OR. ORDSER,'CODSER','SERVICE') IN PER
   CASE ULPR="GAR"
     SET ORDER TO TAG IIF(TYPE("ORDGAR")#'L' .OR. ORDGAR,'CODGAR','GARANTIA') IN PER
   CASE ULPR="PRA"
     SET ORDER TO TAG IIF(TYPE("ORDPRA")#'L' .OR. ORDPRA,'CODPRA','PRACTICA') IN PRA
ENDCASE
RETURN .T.

