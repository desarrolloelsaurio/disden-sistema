#DEFINE PAIP   "|"                               && PAIP
FUNCTION SAYTOT
	EXTERNAL ARRAY TOTAL
	EXTERNAL ARRAY ELPI1
	SYT_SALIDA=""
	FOR AX = 1 TO ALEN(TOTAL)
		IF ISARRAY(@ELPI1)
			SYT_SALIDA=SYT_SALIDA+TRANS(TOTAL[AX],ELPI1[AX])+PAIP
		ELSE
			SYT_SALIDA=SYT_SALIDA+TRANS(TOTAL[AX],ELPI1)+PAIP
		ENDIF
	ENDFOR
RETURN SYT_SALIDA
