* MODIF 01-02-94 07:39pm
FUNCTION Q_TIPTEX
PARAMETER Q_TEX
PRIVATE Q_TEX,Q_SALIDA,Q_TE1,Q_TE2,Q_SALIDA,Q_TE5,Q_TPREV
Q_TEX=ALLTRIM(Q_TEX)
Q_TE1=LEFT(Q_TEX,3)
Q_TE2=VAL(SUBSTR(Q_TEX,6,2))
Q_TE5=VAL(SUBSTR(Q_TEX,4,2))
Q_TPREV=AT("ð",Q_TEX)
IF EMPTY(Q_TPREV)
   Q_SALIDA=""
ELSE   
   Q_SALIDA=ALLTRIM(SUBSTR(Q_TEX,Q_TPREV+1))+"/"
   Q_TEX=LEFT(Q_TEX,Q_TPREV-1)
ENDIF
DO CASE
CASE Q_TE1="EXP"
   Q_SALIDA=Q_SALIDA+"EXPENSAS "+QMES(Q_TE5)+"/"+RIGHT(Q_TEX,2)
CASE Q_TE1="ALQ"
   Q_SALIDA=Q_SALIDA+"ALQUILER "+QMES(Q_TE5)+"/"+RIGHT(Q_TEX,2)
CASE Q_TE1="INM"
   Q_SALIDA=Q_SALIDA+"INMOBIL. PER "+ALLTRIM(STR(Q_TE5))+"/"+SUBSTR(Q_TEX,6,2)+;
   IIF(LEN(Q_TEX)=8," CUOTA "+RIGHT(Q_TEX,1),"")
CASE Q_TE1="MUN"
   Q_SALIDA=Q_SALIDA+"MUNIC. PER "+ALLTRIM(STR(Q_TE5))+"/"+SUBSTR(Q_TEX,6,2)+;
   IIF(LEN(Q_TEX)=8," CUOTA "+RIGHT(Q_TEX,1),"")
CASE Q_TE1="DIP"
   Q_SALIDA=Q_SALIDA+"DIPOS PER "+ALLTRIM(STR(Q_TE5))+"/"+SUBSTR(Q_TEX,6,2)+;
   IIF(LEN(Q_TEX)=8," CUOTA "+RIGHT(Q_TEX,1),"")
CASE Q_TE1="INT"
   Q_SALIDA="INTERESES "+Q_TIPTEX(LEFT(Q_TEX,Q_TPREV-1))
OTHERWISE
   Q_SALIDA=ALLTRIM(Q_TEX)
ENDCASE
RETURN Q_SALIDA
