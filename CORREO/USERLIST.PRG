* PRESENTA UNA LISTA PARA SELECCIONAR EL NOMBRE DEL USUARIO  *
**************************************************************
DIME USUARIOS[2]
M.LALETRA=HAYUSR()
IF EMPTY(M.LALETRA)
RETURN ""
ELSE
	STORE 0 TO RESPUESTA
	=ASORT(USUARIOS)
	STORE 0 TO CANTBOR
	DO WHILE LEFT(USUARIOS[1,1],1)="."
		=ADEL(USUARIOS,1)
		CANTBOR=CANTBOR+1
	ENDDO
	CANTTOT=ALEN(USUARIOS,1)
	NUEVACAN=CANTTOT-CANTBOR
	DIME USUARIOS[NUEVACAN,5]
	RESPUESTA=POP_LIST(10,10,@USUARIOS,'')
RETURN IIF(EMPTY(RESPUESTA) .OR. LAST()=27,'',USUARIOS[RESPUESTA,1])
ENDIF
