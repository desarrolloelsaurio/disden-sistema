FUNCTION CARPRA
PARAMETER C_VENT
   SELE PRA
   SCAT MEMV MEMO BLAN
   SCAT TO RESPALDO MEMO BLAN
   STORE M.CODPRA TO M.CODD
   STORE .T. TO INGRESO
   IF XPRA .AND. ;
      !EMPTY(I_ALIAS)
      SELE PRX
      SCAT MEMV MEMO BLAN
      SCAT TO RESPALDO MEMO BLAN
      STORE M.CODD TO M.CODPRA
   ENDIF
RETURN SGET("PRA") .AND. ;
IIF(XPRA,SGET("XPRA",.F.,.T.),.T.)
