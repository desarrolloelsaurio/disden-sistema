CLEA
=OPMER()
do oppre
SET NEAR ON
sele mer
SCAN
   SELE PRE
   IF XSEEK(left(mer.ARTICULO,10),'PRE','PRECIOS')
      ? mer.articulo
      REPL MER.COSTO WITH COSTO
      REPL MER.COSTOREAL WITH COSTOREAL
      REPL MER.PRECIO1 WITH PRECIO1
      REPL MER.PRECIO2 WITH PRECIO2
      REPL MER.PRECIO3 WITH PRECIO3
      REPL MER.PRECIO4 WITH PRECIO4
   ENDIF
   SELE MER
ENDSCAN
SET NEAR OFF
SET TALK OFF
RETURN
