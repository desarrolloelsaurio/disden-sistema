FUNCTION GETPATH
RELE PATH,PATHPRO,PATHCOM
PUBL PATH,PATHPRO,PATHCOM
DO CASE
CASE GETENV("EQUIPO")="MARISA" .or. GETENV("EQUIPO")="GOYOLAP" 
STORE "C:\SISTEMAS\HORAS\HORASA" TO PATH,PATHPRO,PATHCOM
CASE GETENV("USERNAME")="GOYO"
STORE "G:\SISTEMAS\HORAS\HORASA" TO PATH,PATHPRO,PATHCOM
OTHERWISE
STORE .F. TO PATH,PATHPRO,PATHCOM
ENDCASE
RETURN .T.
