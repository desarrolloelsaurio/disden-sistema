DO OPJAN
SET ORDER TO
SET PRINTER TO FILE E:\SUCIO.TXT
SET PRINTER ON
SET DEVICE TO PRIN
STORE 1 TO C
SCAN
	? 'GOTO '
	?? STR(C,2)
	? 'IF EMPRESA='
A=ALLTRIM(EMPRESA)
DO WHILE .NOT. EMPTY(A)
   ?? 'CHR('
   B=LEFT(A,1)
   B=ASC(B)
   ?? ALLTRIM(STR(B))
   ?? ')+'
   A=SUBSTR(A,2)
  ENDDO
  ?
  C=C+1
ENDSCAN
DO ERROR
RETURN
