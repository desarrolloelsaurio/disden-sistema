function FIRULET1
parameter r,c,r1,c1,tp
private r,c,r1,c1,tp,LARGO,ANCHO,ANCHO1,LARGO1
ANCHO=C1-C+1
LARGO=R1-R+1
ANCHO=ANCHO+3-ANCHO%3
ANCHO1=ANCHO/3
LARGO=LARGO+3-LARGO%3
LARGO1=LARGO/3
RANT=R
@ R,C SAY 'É'+REPL("Í",ANCHO1-2)+'»'+SPACE(ANCHO1)+'É'+REPL("Í",ANCHO1-2)+'»'
R=R+1
FOR B=2 TO LARGO1-1
@ R,C SAY 'º'+SPACE(ANCHO1-2)+'º'+SPACE(ANCHO1)+'º'+SPACE(ANCHO1-2)+'º'
R=R+1
ENDFOR
@ R,C SAY 'È'+REPL("Í",ANCHO1-4)+'» È'+REPL("Í",ANCHO1)+'¼ É'+REPL("Í",ANCHO1-4)+'¼'
R=R+1
FOR B=1 TO LARGO1
@ R,C SAY SPACE(ANCHO1-3)+'º'+SPACE(ANCHO1+4)+'º'
R=R+1
ENDFOR
@ R,C SAY 'É'+REPL("Í",ANCHO1-4)+'¼ É'+REPL("Í",ANCHO1)+'» È'+REPL("Í",ANCHO1-4)+'»'
R=R+1
FOR B=2 TO LARGO1-1
@ R,C SAY 'º'+SPACE(ANCHO1-2)+'º'+SPACE(ANCHO1)+'º'+SPACE(ANCHO1-2)+'º'
R=R+1
ENDFOR
@ R,C SAY 'È'+REPL("Í",ANCHO1-2)+'¼'+SPACE(ANCHO1)+'È'+REPL("Í",ANCHO1-2)+'¼'
R=R+1
@ RANT+1,C+1,RANT+LARGO1-2,C+ANCHO1-2 BOX BOXSTR(1)
@ RANT+1,C+ancho1+1,RANT+LARGO1-2,C+2*ANCHO1-2 BOX BOXSTR(1)
@ RANT+1,C+2*ancho1+1,RANT+LARGO1-2,C+3*ANCHO1-2 BOX BOXSTR(1)
***
@ RANT+1+2*largo1,C+1,RANT+3*LARGO1-2,C+ANCHO1-2 BOX BOXSTR(1)
@ RANT+1+2*largo1,C+ancho1+1,RANT+3*LARGO1-2,C+2*ANCHO1-2 BOX BOXSTR(1)
@ RANT+1+2*largo1,C+2*ancho1+1,RANT+3*LARGO1-2,C+3*ANCHO1-2 BOX BOXSTR(1)
RETURN(.T.)
