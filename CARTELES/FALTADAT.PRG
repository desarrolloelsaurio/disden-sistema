FUNCTION FALTADAT
	PARAMETER F_DAT
	F_LAVEN=WONTOP()
	ACTI SCRE
	=CARTEL("NO SE HA PODIDO REALIZAR LA OPERACION DE",;
		IIF(INGRESO,"INGRESO","CORRECCION")+" DE "+F_DAT,;
		"DADO QUE FALTAN DATOS FUNDAMENTALES",;
		"POR FAVOR, REVISE Y REINTENTE")
	IF !EMPTY(F_LAVEN)
		ACTI WIND (F_LAVEN)
	ENDIF
RETURN .T.
