#DEFINE PIC30 "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"	 && PIC30
#DEFINE CRLF CHR(13)+CHR(10)                     && CRLF
function brorub4
IF !EMPTY(RUB3.RUB3)
PROGRAM=HORA_ULP(PROGRAM())
PUSH KEY CLEAR
ON KEY LABEL ENTER DO BRORUB5
ON KEY LABEL INS DO RUB
ON KEY LABEL DEL DO DELRUB
ON KEY LABEL CTRL+I DO ICONT WITH .F.,4
ON KEY LABEL CTRL+B DO BROCONT WITH 4
ON KEY LABEL ALT+I DO ICONT WITH .T.,4
SELE RUB4
SET ORDER TO TAG RUBRO
DEFI WIND FOUR FROM 4,16 TO 4+LARGO,16+ANCHO SHAD ;
TITL ALLTRIM(RUB3.RUBRO3)+" NIVEL 4 " ;
COLO SCHE 10
BROW FIEL RUBRO4:H=PADC("RUBRO NIVEL 4",30):P=PIC30 ;
WIND FOUR ;
FOR RUB4.RUB1=RUB3.RUB1 .AND. ;
RUB4.RUB2=RUB3.RUB2 .AND. ;
RUB4.RUB3=RUB3.RUB3 ;
NOMENU NOAPPE NODELETE ;
FONT "ARIAL",16
POP KEY
ENDIF
RETURN AYUDA()
