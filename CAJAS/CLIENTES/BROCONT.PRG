#DEFINE PIC40 "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"	 && PIC40
FUNCTION BROCONT
PARAMETER ELN
PUSH KEY CLEAR
=AYUDA(7)
ELN=IIF(EMPTY(ELN),6,ELN)
ON KEY LABEL ENTER DO ICONT WITH .F.,ELN
ON KEY LABEL INS DO ICONT WITH .F.,ELN
ON KEY LABEL DEL DO DELCAJ
ON KEY LABEL ALT+B DO ICONT WITH .T.,ELN
SELE CTN
SET ORDER TO TAG RUBRO
DEFI WIND SEVEN FROM 13,11 TO 20,21+ANCHO+18 SHAD ;
TITL " CONTENIDO DE CAJAS " ;
COLO SCHE 10
FISC="CTN.RUB1="+ALLTRIM(STR(RUB1.RUB1))
FISC=FISC+" .AND. "+IIF(ELN>1,"CTN.RUB2="+ALLT(STR(RUB2.RUB2)),"EMPTY(CTN.RUB2)")
FISC=FISC+" .AND. "+IIF(ELN>2,"CTN.RUB3="+ALLT(STR(RUB3.RUB3)),"EMPTY(CTN.RUB3)")
FISC=FISC+" .AND. "+IIF(ELN>3,"CTN.RUB4="+ALLT(STR(RUB4.RUB4)),"EMPTY(CTN.RUB4)")
FISC=FISC+" .AND. "+IIF(ELN>4,"CTN.RUB5="+ALLT(STR(RUB5.RUB5)),"EMPTY(CTN.RUB5)")
FISC=FISC+" .AND. "+IIF(ELN>5,"CTN.RUB6="+ALLT(STR(RUB6.RUB6)),"EMPTY(CTN.RUB6)")
BROW FIEL CONTENIDO:H=PADC("CONTENIDO",50):P=PIC40 ,;
CAJA:P='9999' ;
FOR &FISC ;
WIND SEVEN ;
NOMENU NOAPPE NODELETE ;
FONT "ARIAL",16
POP KEY
RETURN AYUDA()
