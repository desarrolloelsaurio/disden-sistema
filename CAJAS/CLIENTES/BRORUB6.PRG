#DEFINE PIC30 "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"	 && PIC30
#DEFINE CRLF CHR(13)+CHR(10)                     && CRLF
function brorub6
IF !EMPTY(RUB5.RUB5)
PROGRAM=HORA_ULP(PROGRAM())
PUSH KEY CLEAR
ON KEY LABEL ENTER DO BROCONT WITH 6
ON KEY LABEL INS DO RUB
ON KEY LABEL DEL DO DELRUB
ON KEY LABEL CTRL+I DO ICONT WITH .F.,6
ON KEY LABEL CTRL+B DO BROCONT WITH 6
ON KEY LABEL ALT+I DO ICONT WITH .T.,6
SELE RUB6
SET ORDER TO TAG RUBRO
DEFI WIND SIX FROM 6,26 TO 6+LARGO,26+ANCHO SHAD ;
TITL ALLTRIM(RUB5.RUBRO5)+" NIVEL 6 " ;
COLO SCHE 10
BROW FIEL RUBRO6:H=PADC("RUBRO NIVEL 6",30):P=PIC30 ;
WIND  SIX ;
FOR RUB6.RUB1=RUB5.RUB1 .AND. ;
RUB6.RUB2=RUB5.RUB2 .AND. ;
RUB6.RUB3=RUB5.RUB3 .AND. ;
RUB6.RUB4=RUB5.RUB4 .AND. ;
RUB6.RUB5=RUB5.RUB5 ;
NOMENU NOAPPE NODELETE ;
FONT "ARIAL",16
POP KEY
ENDIF
RETURN AYUDA()
