#DEFINE PIC30 "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"	 && PIC30
#DEFINE CRLF CHR(13)+CHR(10)                     && CRLF
FUNCTION BRORUB3
IF !EMPTY(RUB2.RUB2)
PROGRAM=HORA_ULP(PROGRAM())
PUSH KEY CLEAR
ON KEY LABEL ENTER DO BRORUB4
ON KEY LABEL INS DO RUB
ON KEY LABEL DEL DO DELRUB
ON KEY LABEL CTRL+I DO ICONT WITH .F.,3
ON KEY LABEL CTRL+B DO BROCONT WITH 3
ON KEY LABEL ALT+I DO ICONT WITH .T.,3
SELE RUB3
SET ORDER TO TAG RUBRO
DEFI WIND THREE FROM 3,11 TO 3+LARGO,11+ANCHO SHAD ;
TITL ALLTRIM(RUB2.RUBRO2)+" NIVEL 3 " ;
COLO SCHE 10
BROW FIEL RUBRO3:H=PADC("RUBRO NIVEL 3",30):P=PIC30 ;
WIND THREE ;
FOR RUB3.RUB1=RUB2.RUB1 .AND. ;
RUB3.RUB2=RUB2.RUB2 ;
NOMENU NOAPPE NODELETE ;
FONT "ARIAL",16
POP KEY
ENDIF
RETURN
