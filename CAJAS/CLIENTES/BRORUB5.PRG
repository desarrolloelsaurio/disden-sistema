#DEFINE PIC30 "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"	 && PIC30
#DEFINE CRLF CHR(13)+CHR(10)                     && CRLF
function brorub5
IF !EMPTY(RUB4.RUB4)
PROGRAM=HORA_ULP(PROGRAM())
PUSH KEY CLEAR
ON KEY LABEL ENTER DO BRORUB6
ON KEY LABEL INS DO RUB
ON KEY LABEL DEL DO DELRUB
ON KEY LABEL CTRL+I DO ICONT WITH .F.,5
ON KEY LABEL CTRL+B DO BROCONT WITH 5
ON KEY LABEL ALT+I DO ICONT WITH .T.,5
SELE RUB5
SET ORDER TO TAG RUBRO
DEFI WIND FIVE FROM 5,21 TO 5+LARGO,21+ANCHO SHAD ;
TITL ALLTRIM(RUB4.RUBRO4)+" NIVEL 5 " ;
COLO SCHE 10
BROW FIEL RUBRO5:H=PADC("RUBRO NIVEL 5",30):P=PIC30 ;
WIND FIVE ;
FOR RUB5.RUB1=RUB4.RUB1 .AND. ;
RUB5.RUB2=RUB4.RUB2 .AND. ;
RUB5.RUB3=RUB4.RUB3 .AND. ;
RUB5.RUB4=RUB4.RUB4 ;
NOMENU NOAPPE NODELETE ;
FONT "ARIAL",16
POP KEY
ENDIF
RETURN AYUDA()
