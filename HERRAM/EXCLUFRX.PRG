PROCEDURE EXCLUFRX
PRIVATE DRIVE,A,CANTIDAD,messG,PREVESC,PREVDELE,PREVPATH,PREVTALK
***** messages ***********************
dime MESSG[5]
MESSG[1]='PROJECT'
MESSG[2]='DRIVE'
MESSG[3]='DIRECTORY'
MESSG[4]='SOURCE PROJECT TO TRANSFER'
MESSG[5]=' NOW TRANSFERING '
*************************************
set talk off
set esca on
SET DELE ON
SET PATH TO \SISTEMAS\HERRAM,\SISTEMAS\VENTANAS
SET SAFE OFF
clea
DEFI WIND WIN FROM 0,0 TO 6,70 DOUB
=CENTWIN("WIN")
@ 1,1 SAY MESSG[1]
@ $+1,1 SAY MESSG[2]
@ $+1,1 SAY MESSG[3]
proyecto=getfile("pjx",MESSG[4])
IF !EMPTY(PROYECTO) .AND. LAST()#27
   @ 1,15 SAY PROYECTO
   @ $+1,15 GET DRIVE PICT 'X' DEFA " "
   READ
   IF LAST()#27 .AND. !EMPTY(DRIVE)
         use (PROYECTO)
         REPL EXCLUDE WITH .T. FOR !DELE() .AND. UPPER(TYPE)="R"
         USE
   ENDIF
ENDIF
RELE WIND WIN
SET PATH TO
CLEA
RETURN
