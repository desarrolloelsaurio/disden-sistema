CLOSE DATA
SET SAFE OFF
SET TALK WIND
SET TALK ON
CLEA
proj=getf("PJX")
!DIR >NADA.TXT
CREATE TABLE FILES.DBF ;
		(name C(12))
USE FILES.DBF ALIAS CTRL
APPE FROM NADA.TXT SDF
SELE 0
USE (PROJ) ALIAS PROJ
INDEX ON PADR(LEFT(NAME,AT(".",NAME)-1),8) TO NADA
SET INDEX TO NADA
CLEA
SELE CTRL
GO TOP
DELE NEXT 4
GO BOTT
DELE RECO RECNO()
DELE FOR RIGHT(NAME,3)="TMP" .OR. ;
RIGHT(NAME,1)="K" .OR. ;
RIGHT(NAME,3)="DBF" .OR. ;
RIGHT(NAME,3)="TXT" .OR. ;
RIGHT(NAME,3)="CDX" .OR. ;
RIGHT(NAME,3)="FPT" .OR. ;
RIGHT(NAME,3)="MEM" .OR. ;
RIGHT(NAME,3)="PJT" .OR. ;
RIGHT(NAME,3)="FXP" .OR. ;
RIGHT(NAME,3)="IDX" .OR. ;
RIGHT(NAME,3)="ARJ" .OR. ;
RIGHT(NAME,3)="ERR" .OR. ;
RIGHT(NAME,3)="APP" .OR. ;
RIGHT(NAME,3)="EXE" .OR. ;
RIGHT(NAME,3)="PJX"
PACK
SCAN
A=PADR(ALLTRIM(CTRL.NAME),8)
? A+"=>"
SELE PROJ
IF SEEK(A)
   ?? " ENCONTRE"
   SELE CTRL
   DELE RECO RECNO()
ELSE
   ?? "---------------------------> NO"   
ENDIF
SELE CTRL
ENDSCAN
PACK
LIST TO SACAR.TXT
RETURN