FUNCTION GETPATH
IF GETENV("EQUIPO")="MICASA"
   STORE "D:\SISTEMAS\LABORATO\LABORATA" TO PATH,PATHPRO,PATHVEN,PATHLAB
ELSE
   IF GETENV("EQUIPO")="CAMARA" .OR. ;
        GETENV("EQUIPO")="GOYOLAP"
      STORE "C:\SISTEMAS\LABORATO\LABORATA" TO PATH,PATHPRO,PATHVEN,PATHLAB
   ELSE
      IF GETENV("EQUIPO")="CAMARED"
         STORE "F:\SISTEMAS\LABORATO\LABORATA" TO PATH,PATHPRO,PATHVEN,PATHLAB
      ENDIF
   ENDIF
ENDIF
RETURN .T.
