FUNCTION ELIUNIX
PARA EL_MSG,EL_R1,EL_C1,XTIPO
SELE MIX
XTIPO=IIF(TYPE("XTIPO")="C",LEFT(XTIPO,3),M.TIPO)
IF XTIPO="PRA"
   RETURN ELIPRA(EL_MSG,EL_R1,EL_C1)
ELSE
VARIPOP=XTIPO+"POP"
VARIORD=XTIPO+"TEX"
VARIORD1=XTIPO+"COD"
IF EVAL(VARIPOP)
   SET ORDER TO TAG (VARIORD)
   ACTI POPUP MIXpop
   STOR !EMPTY(RESPUESTA) TO RESPUESTA
ELSE
   RESPUESTA=ELIMIX1(QTIPCON(XTIPO)+EL_MSG,'MIX',VARIORD1)
ENDIF
ENDIF
RETURN RESPUESTA
