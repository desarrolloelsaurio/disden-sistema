FUNCTION GETJANNA
IF GETENV("EQUIPO")='ARGENTA' .OR. GETENV("EQUIPO")='DESARROLLO' 
   JANNAME=CHR(65)+CHR(114)+CHR(103)+CHR(101)+CHR(110)+CHR(116)+CHR(97)
   *** ARGENTA
   SUCU='0000'
ELSE
   JANNAME=CHR(68)+CHR(105)+CHR(32)+CHR(80)+CHR(97)+CHR(111)+CHR(108)+CHR(111) 
   *** DIPAOLO
   SUCU='0000'
ENDIF
RETURN .T.
