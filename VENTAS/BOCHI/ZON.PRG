do case
case bar()=1
   do izon
case bar()=2
   SELE ZON
   ACTI POPU ZONPOP
   IF !EMPTY(RESPUESTA)
      GO TOP
      SKIP RESPUESTA-1
      DO CZON WITH RECNO()
   ENDI
Case bar()=3
   SELE ZON
   SET ORDER TO TAG ZONA
   DEFI WIND CACA FROM 1,0 TO 5,79 DOUB
   ACTI WIND CACA
   DO TIT2 WITH 1,' LISTADO DE ZONAS ','I',78
   DEFI WIND WIN FROM 5,0 TO 21,79 DOUB
   BROWSE NOINIT NOFOLLOW NOAPPEND NOEDIT NODELETE FIELDS ZONCOD,ZONA WIND WIN
   DEAC WIND ALL
ENDC
return

