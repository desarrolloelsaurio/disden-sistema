*-- Notas   : DERIVA DEL POPU DE PROVEEDORES
*--           VIENE DEL MENU PRINCIPAL
********************************************************************************
SELE PRO
SET ORDER TO TAG FIRMA
 
DO CASE
CASE BAR()=1
   DO INDATPRO
CASE BAR()=2
   ACTI POPU PROPOP
   IF !EMPTY(RESPUESTA)
      GO TOP
      SKIP RESPUESTA-1
      DO CODATPRO WITH RECNO()
   ENDI
CASE BAR()=4
   DO LIPROPAO
CASE BAR()=5
   DEFI WIND CACA FROM 1,0 TO 5,79 DOUB
   ACTI WIND CACA
   DO TIT2 WITH 1,' LISTADO DE PROVEEDORES ','I',78
   DEFI WIND WIN FROM 5,0 TO 21,79 DOUB
   BROWSE NOINIT NOFOLLOW NOAPPEND NOEDIT NODELETE FIELDS FIRMA,APELLIDO,TELEFONO,TELEFONO1,LOCALIDAD WIND WIN
   DEAC WIND ALL
CASE BAR()=6
   DEFI WIND CACA FROM 1,0 TO 5,79 DOUB
   ACTI WIND CACA
   DO TIT2 WITH 1,' LISTADO DE PROVEEDORES ','I',78
   DEFI WIND WIN FROM 5,0 TO 21,79 DOUB
   BROWSE NOINIT NOFOLLOW NOAPPEND NOEDIT NODELETE WIND WIN
   DEAC WIND ALL
CASE BAR()=7
   DO LIPROPAI
   * CASE BAR()=8
   *      LABEL
CASE BAR()=10
   DEFI POPU CLI1 FROM 10,40 MESS 'Men£ de Proveedores - Marca Proveedores '
   DEFI BAR 1 OF CLI1 PROM ' Marca Proveedores Argentinos '
   DEFI BAR 2 OF CLI1 PROM ' Marca Proveedores Extranjeros '
   DEFI BAR 3 OF CLI1 PROM ' Marca Por Localidad '
   DEFI BAR 4 OF CLI1 PROM ' Marca por Ficha '
   ON SELE POPU CLI1 DO MARPRO
   ACTI POPU MARPRO
CASE BAR()=11
   DO DESMACLI
ENDC
RETURN
