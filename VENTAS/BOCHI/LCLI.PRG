PROCEDURE LCLI
DIJO=BAR()
DO CASE
CASE DIJO=1
   SET ORDER TO TAG APELLIDO
   DO LICLIPAO
CASE DIJO=2
   SET ORDER TO TAG APELLIDO
   DEFI WIND CACA FROM 1,0 TO 5,79 DOUB
   ACTI WIND CACA
   DO TIT2 WITH 1,' LISTADO DE CLIENTES ','I',78
   DEFI WIND WIN FROM 5,0 TO 21,79 DOUB
   BROWSE NOINIT NOFOLLOW NOAPPEND NOEDIT NODELETE FIELDS APELLIDO,NOMBRE,DOMIROS,TELROS,LOCAROS,CODROS WIND WIN
   DEAC WIND ALL
CASE DIJO=3
   SET ORDER TO TAG APELLIDO
   DEFI WIND CACA FROM 1,0 TO 5,79 DOUB
   ACTI WIND CACA
   DO TIT2 WITH 1,' LISTADO DE CLIENTES ','I',78
   DEFI WIND WIN FROM 5,0 TO 21,79 DOUB
   BROWSE NOINIT NOFOLLOW NOAPPEND NOEDIT NODELETE FIELDS APELLIDO,NOMBRE,DOMIINT,TELINT,LOCAINT,CODINT WIND WIN
   DEAC WIND ALL
CASE DIJO=4
   SET ORDER TO TAG APELLIDO
   DEFI WIND CACA FROM 1,0 TO 5,79 DOUB
   ACTI WIND CACA
   DO TIT2 WITH 1,' LISTADO DE CLIENTES ','I',78
   DEFI WIND WIN FROM 5,0 TO 21,79 DOUB
   BROWSE NOINIT NOFOLLOW NOAPPEND NOEDIT NODELETE FIELDS APELLIDO,NOMBRE,DOMIROS,TELROS,LOCAROS,CODROS,DOMIINT,TELINT,LOCAINT,CODINT,DETALLE WIND WIN
   DEAC WIND ALL
CASE DIJO=5
DEFI POPU CLI2 FROM 13,40 MESS 'Men£ de Clientes - Listados de Clientes'
DEFI BAR 1 OF CLI2 PROM ' Todo el Archivo'
DEFI BAR 2 OF CLI2 PROM ' Desde Cliente N§ '
ON SELE POPU CLI2 DO LICLIPAI
ACTI POPU CLI2
DEAC POPU
CASE DIJO=6
DEFI POPU CLI2 FROM 13,40 MESS 'Men£ de Clientes - Listados de Cumplea¤os'
DEFI BAR 1 OF CLI2 PROM ' Clientes Sin Datos '
DEFI BAR 2 OF CLI2 PROM ' Listado por Fecha'
ON SELE POPU CLI2 DO LICLICUM
ACTI POPU CLI2
DEAC POPU
CASE DIJO=7
*   set console off
*   SET PRINTER ON
*   LABEL FORM CLI
*   SET PRINTER OFF
*   set console on
ENDCASE
RETURN .T.
