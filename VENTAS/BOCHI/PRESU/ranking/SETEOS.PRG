FUNCTION SETEOS
CLEAR MACROS
#IF "Wind" $ VERS()
		=HAZBMP()
		MODIFY WIND SCREEN FONT "ARIAL",8
	#ENDIF
	RELE ALL
	CLOSE ALL
	CLEA ALL
	SET DOHI OFF
	SET CURRENCY TO
	SET ODOM TO 1
	SET EXCL OFF
	SET STAT OFF
	SET ESCA OFF
	SET TALK OFF
	SET HELP OFF
	SET FULL ON
	SET REPR TO 5
	SET DEVE ON
	SET MEMO TO 70
	SET DECIMALS TO 4
	SET LOGE OFF
	SET SAFE OFF
	SET OPTI ON
	SET DELE ON
	SET EXCL OFF
	SET CONFIRM ON
	SET MULTI ON
	SET DATE TO FRENCH
	SET BELL TO 100,1
	SET AUTOSAVE ON
	SET MOUSE ON
	SET SHAD ON
	SET SYSMENU OFF
	IF GETENV("EQUIPO")#"MICASA" .and. ;
			GETENV("EQUIPO")#"GOYOLAP"
		IF GETENV("USERNAME")#"GOYO" .AND. ;
				GETENV("USERNAME")#"BATA"
			ON ERROR DO ERROR
		ELSE
			ON ERROR
			SET ESCA ON
			SET DEVE ON
			IF _WINDOWS
				SET STATUS BAR OFF
			ENDIF
		ENDIF
	ELSE
		ON ERROR
		SET ESCA ON
	ENDIF
	=GETFUSER()
	=IIF(!_WINDOWS,BOX(),.T.)
RETURN .T.
