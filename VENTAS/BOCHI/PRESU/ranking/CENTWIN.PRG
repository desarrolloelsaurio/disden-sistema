FUNCTION CENTWIN
PARAMETER VENTANA
if parameter()<1
   ventana='win'
endif   
#IF "Wind" $ VERS()
COLS=INT((SCOLS()-WCOLS((VENTANA)))/2)
RENS=INT((SROWS()-WROWS((VENTANA)))/2)
#ELSE
COLS=INT((79-WCOLS((VENTANA)))/2)
RENS=INT((24-WROWS((VENTANA)))/2)
#ENDIF
MOVE WIND (VENTANA) TO RENS,COLS
ACTI WIND (VENTANA)
RETURN .T.