FUNCTION GETNU1
	PARAMETER P_TEXTO,P_ORD1,P_ELNU
	P_BASANT=ALIAS()
	P_ORD1=IIF(EMPTY(P_ORD1),"FOXGRL",P_ORD1)
	P_ORDER=IIF(P_BASANT="MIX",ORDER(),"")
	SELE MIX
	DO CASE
	CASE EMPTY(P_ELNU) .OR. P_ELNU=1
		P_SALIDA=IIF(XSEEK(P_TEXTO,"MIX",P_ORD1),MIX.NU1,0)
	CASE P_ELNU=2
		P_SALIDA=IIF(XSEEK(P_TEXTO,"MIX",P_ORD1),MIX.NU2,0)
	CASE P_ELNU=3
		P_SALIDA=IIF(XSEEK(P_TEXTO,"MIX",P_ORD1),MIX.NU3,0)
	CASE P_ELNU=4
		P_SALIDA=IIF(XSEEK(P_TEXTO,"MIX",P_ORD1),MIX.NU4,0)
	CASE P_ELNU=5
		P_SALIDA=IIF(XSEEK(P_TEXTO,"MIX",P_ORD1),MIX.NU5,0)
	ENDCASE
	IF !EMPTY(P_BASANT)
		SELE (P_BASANT)
		IF !EMPTY(P_ORDER)
			SET ORDER TO TAG (P_ORDER)
		ENDIF
	ENDIF
RETURN P_SALIDA
