function SAYJANNA
parameter solodato
IF GETENV("VIEJO")="SI"
   IF GETENV("EQUIPO")="ARROYO"
   EMPRESA1="BLAS MARTINUCCI"
   EMPRESA2="ARROYOGAR"
   EMPRESA3="SISTEMA VIEJO"
   ELSE
   EMPRESA1="BLAS MARTINUCCI"
   EMPRESA2="NOVOGAR"
   EMPRESA3="CUIT: 20-06008964-8"
   ENDIF
ELSE
   DO CASE
   CASE GETENV("EQUIPO")="ARROYO"
      EMPRESA2="ARROYOGAR"
   CASE GETENV("EQUIPO")="EQUIPA"
         EMPRESA2="NOVOGAR"
*      EMPRESA2="EQUIPA HOGAR"
   OTHERWISE
   EMPRESA2="NOVOGAR"
   ENDCASE      
   EMPRESA1="BLAS OSCAR MARTINUCCI E HIJOS S.A."
   EMPRESA3="CUIT: 30-68543701-1"
ENDIF   
if solodato
   return empresa1
else   
=CART(10,16,20,60)
   @ 10,21 SAY PADC(EMPRESA1,38)
   @ 12,21 SAY PADC(EMPRESA2,38)
   @ 14,21 SAY PADC(EMPRESA3,38)
endif
RETURN .T.