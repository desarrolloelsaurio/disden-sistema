FUNCTION LPRO0
	?paip
	=iif(variable[1],qout(PROCOD+paip),'')
	=iif(variable[2],qout(PROVEEDOR+paip),'')
	=IIF(VARIABLE[3],QOUT(LEFT(encVENTAS,TEXTO[3,2])+PAIP),'')
	=IIF(VARIABLE[4],QOUT(LEFT(VIAJANTE,TEXTO[4,2])+PAIP),'')

	=IIF(VARIABLE[5],QouT(domicilio+PAIP),'')
	IF VARIABLE[6]
		=XSEEK(LOCALIDAD,'MIX','CIUCOD')
		=QOUT(LEFT(MIX.TEXTO1,TEXTO[6,2])+PAIP)
	ENDIF
	IF VARIABLE[7]
		=XSEEK(PAIS,'MIX','PAICOD')
		=QOUT(LEFT(MIX.TEXTO1,TEXTO[7,2])+PAIP)
	ENDIF
	=IIF(VARIABLE[8],QOUT(telefono+PAIP),'')
	=IIF(VARIABLE[9],QOUT(fax+PAIP),'')
	=IIF(VARIABLE[10],QOUT(DETALLE+PAIP),'')
	=IIF(VARIABLE[11],QOUT(padr(abrev(XACIVA[condiva,1]),TEXTO[11,2])+PAIP),'')
	=IIF(VARIABLE[12],QOUT(nroiva+PAIP),'')
	=IIF(VARIABLE[13],QOUT(STR(COEF,12,6)+PAIP),'')
	=IIF(VARIABLE[14],QOUT(DESC+PAIP),'')
	=IIF(VARIABLE[15],QOUT(STR(COEFRES,12,6)+PAIP),'')
	=IIF(VARIABLE[16],QOUT(IIF(POLIGONAL,'SI','NO')+PAIP),'')
	CASOS=CASOS+1
RETURN .T.
