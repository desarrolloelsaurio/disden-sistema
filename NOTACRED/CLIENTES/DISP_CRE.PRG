FUNCTION DISP_CRE
	PARAMETER D_UNO,D_DOS,D_TIPO
	DO CASE
	CASE LEFT(JANNAME,7)="NOVOGAR" ;
	.OR. LEFT(JANNAME,8)="NOV DEMO"	;
	.OR. LEFT(JANNAME,9)="ARROYOGAR" ;
	.OR. LEFT(JANNAME,6)="EQUIPA"
	IF XSEEK(FAC.FACTURA,"FAX","FACTURA")
	   IF EMPTY(D_TIPO) .OR. D_TIPO='A'
          DO lfaciA WITH D_UNO,D_DOS,D_TIPO
       ELSE
          DO lfaciB WITH D_UNO,D_DOS,D_TIPO
       ENDIF
	ELSE
	   IF EMPTY(D_TIPO) .OR. D_TIPO='A'
		  DO lcreiA WITH D_UNO,D_DOS
	   ELSE
		  DO lcreiB WITH D_UNO,D_DOS		
	   ENDIF
    ENDIF
	CASE LEFT(JANNAME,5)="PALAD" 
		DO lcrei1P WITH D_UNO,D_DOS
	CASE LEFT(JANNAME,5)="GUILL"
		DO lcrei1GU WITH D_UNO,D_DOS
	CASE RIGHT(ALLTRIM(JANNAME),5)="MELAC"
		DO lCREIME WITH D_UNO,D_DOS
	OTHERWISE
		DO lcrei1L WITH D_UNO,D_DOS
	ENDCASE
RETURN .T.
