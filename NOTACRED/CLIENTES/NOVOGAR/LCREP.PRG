* VERSION NOVOGAR
PARAMETER CHICHO
DO CASE
CASE BAR()=1
ELTIPO='A'
   FIL_INVO=FIL_INVO+' .AND. LEFT(FAC.FACTURA,1)="A"'
   DO LFAC WITH CHICHO
CASE BAR()=2
ELTIPO='B'
   FIL_INVO=FIL_INVO+' .AND. LEFT(FAC.FACTURA,1)="B"'
   DO LFAC WITH CHICHO
CASE BAR()=3
ELTIPO='*'
   DO LFAC WITH CHICHO
ENDCASE
RETURN