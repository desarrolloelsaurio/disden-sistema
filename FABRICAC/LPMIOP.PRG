STORE HORA_ULP(PROGRAM()) TO ULPR
IF CHEQSEG()
	=WIN0()
	PAG=1
	=CABPAN()
	=TIT_LIN(1)
	SELE TEMPO
	SCAN
		IF ROW()>21
			IF !FIN_LIN(1,NUL,FIN_SUM("SUBTOTAL",@TTL))
				EXIT
			ENDIF
			CLEA
			STOR PAG+1 TO PAG
			=CABPAN()
			=TIT_LIN(2)
		ENDIF
		=LPMIO1()
	ENDSCAN
	=FIN_LIN(2,CASOS,FIN_SUM("TOTAL",@TTL))
	=RELEWIN("WIN")
	=IAUD("")
ELSE
	=IAUDI()
ENDIF
RETURN MEN_LIN(BOTOM)
