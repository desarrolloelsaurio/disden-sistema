FUNCTION LFABPT1
	#DEFINE PAIP   "|"                             && PAIP
	? PAIP
	=IIF(VARIABLE[1],QOUT(STR(NUMERO)+PAIP),"")
	=IIF(VARIABLE[2],QOUT(LEFT(ARTICULO,TEXTO[2,2])+PAIP),"")
	=IIF(VARIABLE[3],QOUT(LEFT(MERCA,TEXTO[3,2])+PAIP),"")
	=IIF(VARIABLE[4],QOUT(LEFT(CLIENTE,TEXTO[4,2])+PAIP),"")
	=IIF(VARIABLE[5],QOUT(DTOC(EF)+PAIP),"")
	=IIF(VARIABLE[6],QOUT(DTOC(SF)+PAIP),"")
	=IIF(VARIABLE[7],QOUT(STR(SF-EF,XACDIGI)+PAIP),"")
	IF VARIABLE[8]
		?? STR(EEC,XACDIGI)+PAIP
		TTL[8]=TTL[8]+EEC
	ENDIF
	IF VARIABLE[9]
		?? STR(SSC,XACDIGI)+PAIP
		TTL[9]=TTL[9]+SSC
	ENDIF
	IF VARIABLE[10]
		?? STR(EEC-SSC,XACDIGI)+PAIP
		TTL[10]=TTL[10]+EEC-SSC
	ENDIF
	STORE CASOS+1 TO CASOS
RETURN .T.
