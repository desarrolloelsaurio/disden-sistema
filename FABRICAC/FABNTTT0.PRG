FUNCTION FABNTTT0
PARAMETER FAB_NUM,FAB0TEX,SINTIT
#DEFINE ONE    1                                 && ONE
#DEFINE TWO    2                                 && TWO
STORE CLIENTE TO ELPRC,ELPRCTEX
STORE CASOS+CASOSPARC TO CASOSPARC
STORE TTTOT+TTPARC TO TTTOT
IF NOFIRST
   IF LG_PORP 
      FAB_SALIDA=FIN_LIN(FAB_NUM,CASOSPARC,FABNTRR2(FAB0TEX,TTPARC))   
	  CLEA
      STOR PAG+ONE TO PAG
      =CABPAN()
      =TIT_LIN(TWO,ALLTRIM(ELPRCTEX))      
    ELSE
      =FIN_LIN(FAB_NUM,CASOSPARC,FABNTRR2(FAB0TEX,TTPARC),SINTIT)       
      IF !SINTIT
	  =cabpri1()
      =TIT_LIN(FAB_NUM,ALLTRIM(ELPRCTEX))
      ENDIF
   ENDIF
ELSE
   STORE .T. TO FAB_SALIDA
ENDIF   
STORE .T. TO NOFIRST
STORE NUL TO CASOSPARC,TTPARC
RETURN LG_PORP .AND. !FAB_SALIDA