FUNCTION LTER20
	IF LAMER#COR.ARTICULO .OR. ;
	EMPTY(LAMER)
		? PAIP
		?? LAMER
		?? PAIP
		?? LEFT(SAYGRL(LAMER,"MER","ARTIRED","MER.MERCA"),20)
		?? PAIP
		?? STR(TER1,10)
		?? PAIP
		?? STR(TER2,10)
		?? PAIP
		?? STR(TER1+TER2,10)
		?? PAIP
		?? STR(TER2*100/(TER1+TER2),7,2)
		?? PAIP
		TTER1=TTER1+TER1
		TTER2=TTER2+TER2
		STORE NUL TO TER1,TER2
		LAMER=COR.ARTICULO
	ENDIF
	IF TMP.PRC="99991"
		TER1=TER1+TMP.SSC
	ELSE
		TER2=TER2+TMP.SSC
	ENDIF
RETURN .T.
