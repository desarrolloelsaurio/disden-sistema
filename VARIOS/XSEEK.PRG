FUNCTION XSEEK
PARAMETER QUEBUSCA,APODO,HORDEN,X_NEAR
PRIVATE QUEBUSCA,APODO,HORDEN,SALIDA,APODOANT,HORDENANT
IF PARAMETER()<2
   M.SALIDA=.F.
ELSE
   STORE ALIAS() TO M.APODOANT
   SELE (M.APODO)
   IF PARAMETER()=3
	  M.ORDENANT=ORDER()
	  SET ORDER TO TAG (M.HORDEN)
   ENDIF
   IF X_NEAR
      SET NEAR ON
   ENDIF
   M.SALIDA=SEEK(M.QUEBUSCA)
   IF X_NEAR
      SET NEAR OFF
   ENDIF
   IF PARAMETER()=3
	  SET ORDER TO TAG (M.ORDENANT)
   ENDIF
   IF !EMPTY(M.APODOANT)
      SELE (M.APODOANT)
   ENDIF
ENDIF
RETURN M.SALIDA
