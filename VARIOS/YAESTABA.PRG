* YB_CAMPO    -> INFORMACION A BUSCAR
* YB_ALIAS    -> ALIAS DE LA BASE EN LA QUE SE REALIZARA LA BUSQUEDA
* YB_ORDER    -> ORDER TAG DE LA BUSQUEDA EN LA BASE YB_ALIAS
* YB_ALFA     -> DEVUELVE .F. SI YB_CAMPO NO ES ALFANUMERICO
* YB_UPDA     -> DEVUELVE .F. SI NO HUBO UPDATE()
* YB_EMPTY    -> DEVUELVE .F. SI YB_CAMPO ES EMPTY()
FUNCTION YAESTABA
PARAMETER YB_CAMPO,YB_ALIAS,YB_ORDER,YB_ALFA,YB_UPDA,YB_EMPTY
PRIVATE YB_ESTABA,YB_ALANT
store .t. to M.YB_ESTABA
IF M.YB_EMPTY .AND. EMPTY(YB_CAMPO)
   M.YB_ESTABA=.F.
endif
if M.YB_ESTABA
   if YB_ALFA .and. !isalpha(YB_CAMPO)
	  m.YB_ESTABA=.f.
   endif
endif
if M.YB_ESTABA
   IF YB_UPDA .AND. !UPDATE()
      M.YB_ESTABA=.F.
	 endif
endif
if M.YB_ESTABA 
   M.YB_ALANT=ALIAS()
   SELE (M.YB_ALIAS)
   SET ORDER TO TAG (M.YB_ORDER)
   SET DELE OFF
   M.YB_ESTABA=IIF(!M.YB_ESTABA,.F.,SEEK(YB_CAMPO))
   M.YB_ESTABA=IIF(M.YB_ESTABA,(LAST()#27),.F.)
   SET DELE ON
   SELE (M.YB_ALANT)
ENDIF
RETURN M.YB_ESTABA
