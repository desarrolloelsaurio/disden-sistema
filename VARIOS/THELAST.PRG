FUNCTION THELAST
PARAMETER LABASE,ELTEXTO,elancho,ELCAMPO1,ELTEXTO1,UNTAG,ELCAMPO2,ELTEXTO2,DOSTAG
PRIVATE LABASE,ELTEXTO,ELCAMPO,UNTAG,DOSTAG,ALIASANT,ORDENANT
ALIASANT=ALIAS()
ORDENANT=ORDER()
=MOVEWIN(2,79-elancho,iif(parameter()=6,14,18),79,"THELAST",'Datos')

@ 1,1 GET ELTEXTO

* orden natural
SELE (LABASE)
NADA='ORDEN DE INGRESO'
@ 3,1 GET NADA
SET ORDER TO
GO TOP
nada=EVAL(ELCAMPO1)
@ 4,1 SAY "INICIAL:" GET NADA
GO BOTT
nada=EVAL(ELCAMPO1)
@ 5,1 SAY "FINAL  :" GET NADA

* primer tag
@ 7,1 GET ELTEXTO1
SET ORDER TO TAG (UNTAG)
go top
nada=EVAL(ELCAMPO1)
@ 8,1 SAY "INICIAL:" GET NADA
GO BOTT
nada=EVAL(ELCAMPO1)
@ 9,1 SAY "FINAL  :" GET NADA

* segundo tag
IF !EMPTY(ELTEXTO2)
   @ 11,1 GET ELTEXTO2
   SEt order to tag (DOSTAG)
go top
nada=EVAL(ELCAMPO2)
@ 12,1 SAY "INICIAL:" GET NADA
GO BOTT
nada=EVAL(ELCAMPO2)
@ 13,1 SAY "FINAL  :" GET NADA
ENDIF
CLEA GETS
=INKEY(NUL,'HM')
SELE (ALIASANT)
SET ORDER TO TAG (ORDENANT)
=relewin("THELAST")
RETURN .T.
