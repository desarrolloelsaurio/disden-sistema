FUNCTION ISEQ_DBF
	PARAMETER UNO,DOS
	CLOSE DATA
	SELE 0
	IF TYPE("UNO")='L'
		USE GETF() ALIAS UNO
	ELSE
		USE UNO ALIAS UNO
	ENDIF
	SELE 0
	IF TYPE("DOS")='L'
		USE GETF() ALIAS DOS
	ELSE
		USE DOS ALIAS DOS
	ENDIF
	FOR A=1 TO FCOUNT("UNO")
		U=FIELD(A,'UNO')
		D=FIELD(A,'DOS')
		? U
		?? '-'
		?? D
	NEXT
	? 'LARGO UNO:'
	?? FCOUNT("UNO")
	? 'LARGO DIS:'
	?? FCOUNT("DOS")
RETURN

