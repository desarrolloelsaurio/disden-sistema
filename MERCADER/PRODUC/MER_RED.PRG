FUNCTION MER_RED
	PARA TITUTXT
	DO WHILE .T.
		DO IMER_DO.SPR WITH .F.,TITUTXT,3,.T.
		IF LAST()=27
			RETURN NUL
		ENDIF
		STORE LEFT(MER.ARTICULO,11)+LEFT(MIX.TEXTO2,2) TO M.RESPUESTA , M.ARTICULO
		SELE DES
		IF SEEK(M.ARTICULO)
			EXIT
		ENDIF
		WAIT "NO EXISTE EL DESPIECE" WIND
	ENDDO
RETURN RESPUESTA
