FUNCTION SGET
PARAMETER SHOW_VEN,S_TIPO,SHOW_ACTI
IF !EMPTY(SHOW_VEN)
   IF WEXIST(SHOW_VEN)
      IF PARAMETER()<2
         IF !EMPTY(SHOW_VEN)
            SHOW GETS WIND (SHOW_VEN)
         ENDIF
   ELSE
      IF !WONTOP((SHOW_VEN))
         ACTI WIND (SHOW_VEN)
      ENDIF
      SHOW GETS WIND (SHOW_VEN)
   ENDIF
ENDIF
ENDIF
RETURN .T.
