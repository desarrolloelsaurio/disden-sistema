FUNCTION SGETX
PARAMETER SG_ALIAS
DO CASE
CASE SG_ALIAS="MUT" .OR. ;
     SG_ALIAS="XMUT"
   SHOW GETS WIND MUT
   IF XMUT
      ACTI WIND XMUT
      SHOW GETS WIND XMUT
   ENDIF
CASE SG_ALIAS="PRA"
   SHOW GETS WIND PRA
   IF XPRA
      ACTI WIND XPRA
      SHOW GETS WIND XPRA
   ENDIF
CASE SG_ALIAS="MER"
   SHOW GETS WIND MER
   IF XMER
      ACTI WIND XMER
      SHOW GETS WIND XMER
   ENDIF
CASE SG_ALIAS="INGPRS1"
   SHOW GETS WIND PER
   ACTI WIND INGPRS1
   SHOW GETS WIND INGPRS1
OTHERWISE
   =SGET(YA_WIN)
ENDCASE
RETURN .T.
