FUNCTION LRESPT1
	? PAIP
	FOR A=1 TO CANT_MAX
		?? EVAL(TEXTO[A,3]) PICT TEXTO[A,5]
		?? PAIP
	NEXT
	ELCORTE=EVAL(C_COR)
	ELTEXTO=EVAL(C_TEX)
	FOR A=1 TO C_LAR-IIF(!PROMEDIA,1,0)
		GTL[A]=GTL[A]+TTL[A]
	NEXT
	IF !PROMEDIA
		GTL[A]=GTL[A-1]/GTL[A-2]
	ENDIF
	STORE CASOS+1 TO CASOS
	STORE NUL TO TTL
RETURN .T.
