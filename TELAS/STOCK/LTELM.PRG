FUNCTION LTELM
	EXTERNAL ARRAY TOTAL
	#DEFINE PAIP   "|"                            && PAIP
	? PAIP
	?? TELCODPRO
	?? PAIP
	IF !PORPAN
		?? LEFT(TELA,30)
		?? PAIP
		?? SAYPER(PROCOD,"PROCOD",20)
		?? PAIP
	ENDIF
	?? STR(MONTO,XACDIGI,3)
	?? PAIP
	?? STR(STOCK,XACDIGI)
	STORE TOTAL[1]+STOCK TO TOTAL[1]
	?? PAIP
	?? STR(STOCKRES,XACDIGI)
	STORE TOTAL[2]+STOCKRES TO TOTAL[2]
	?? PAIP
	?? STR(STKENTPEN,XACDIGI)
	STORE TOTAL[3]+STKENTPEN TO TOTAL[3]
	?? PAIP
	LAS=STOCK-STOCKRES-STKRESPEN+STKENTPEN
	?? STR(LAS,XACDIGI)
	STORE TOTAL[4]+LAS TO TOTAL[4]
?? PAIP
RETURN .T.
