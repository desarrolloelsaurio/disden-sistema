     *    IF GETENV("ESTUD")="SI"
     *       IF LEFT(PER.CONDIVA,1)="R"
     *          WAIT "ESTE CLIENTE NO PUEDE FACTURARSE DESDE ESTA MAQUINA" ;
     *             WIND
     *          RETURN .F.
     *       ENDIF
     *    ENDIF
     *    IF EMPTY(GETENV("ESTUD"))
     *       IF LEFT(PER.CONDIVA,1)#"R"
     *          WAIT "ESTE CLIENTE NO PUEDE FACTURARSE DESDE ESTA MAQUINA" ;
     *             WIND
     *          RETURN .F.
     *       ENDIF
     *    ENDIF
         ELIVA=per.CONDIVA
         IF PER.CONDIVA="EX"
            STORE 0 TO IVA1,IVA2
         ELSE
            IVA1=IVA18
