FUNCTION _6wa0ycq76     &&  m.codcli VALID
#REGION 1
IF LAST()=27
   RETURN .T.
ELSE
   IF EMPTY(M.CODCLI)
      RETURN .T.
   ELSE
      M.CODCLI1=M.CODCLI
      M.CODCLI2=AJCODPER(ALLTRIM(M.CODCLI),9,"C")
      IF XSEEK(M.CODCLI1,'per','CODCLI') .OR. ;
          XSEEK(M.CODCLI2,'per','CODCLI')
         M.CODCLI=PER.CODCLI
         SELE per
         SCAT MEMV
        IF PER.CONDIVA="RI" .OR. (PER.CONDIVA="RN" .AND. !BIENUSO)
	   		TIPOCLI = "A"
   		ELSE
   			TIPOCLI = "B"
