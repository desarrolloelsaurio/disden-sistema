FUNCTION QW_EST
PARAMETER REN,COL,CUAL
PRIVATE REN,COL,CUAL
STORE NUL TO EST_SALIDA
IF FILEST=2
   STORE "" TO TT,FISC
   QWSALIDA=QWEST(MIXGRL("Estados","Estados","Estados",.t.),cual)
   IF QWSALIDA#".F."
      QW=AT("ú",QWSALIDA)
      QUEest=LEFT(QWSALIDA,QW-1)
      TTest=SUBSTR(QWSALIDA,QW+1)
   ELSE
      STORE 1 TO FILEST
   ENDIF
ENDIF
RETURN EST_SALIDA#2
