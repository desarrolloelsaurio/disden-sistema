FUNCTION LREMP40
	=WORKING()
	SET ORDER TO TAG PROCOD IN PRO
	SET ORDER TO TAG ARTICULO IN MER
	SELE REM
	SET RELA TO PROVEEDOR INTO PRO
	SET RELA TO MERCA INTO MER ADDI
	stor nul TO casos
	INDE ON PRO.PROVEEDOR+STR(REMESA) TO (GENTMP()) FOR EVAL(FISC) COMP
	acla=HAZ_ACLA()
	linea=REPL("=",LEN(acla))
	m.linea=REPL("=",LEN(acla))
	GO TOP
	ELPRO=REM.PROVEEDOR
    LAREM=REM.REMESA
	=UNWORK()
RETURN .T.
