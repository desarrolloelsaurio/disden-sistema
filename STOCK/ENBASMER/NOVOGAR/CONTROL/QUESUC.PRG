FUNCTION QUESUC
PARAMETER Q_SUC
Q_SUC=IIF(EMPTY(Q_SUC),LASUC,Q_SUC)
Q_SAL=""
DO CASE
CASE Q_SUC="0004"
     Q_SAL="ARROYOGAR"
CASE Q_SUC="0005"
     Q_SAL="NOVOGAR"
CASE Q_SUC="0003"
     Q_SAL="EMPALME"
CASE Q_SUC="0008"
     Q_SAL="25 DE MAYO"
CASE Q_SUC="0007"
     Q_SAL="LIBERTAD"
ENDCASE
RETURN Q_SAL
