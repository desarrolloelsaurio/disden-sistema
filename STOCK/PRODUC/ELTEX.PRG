FUNCTION ELTEX
#IF "NOVO" $ CURD()
RETURN SAYMER(ARTICULO,"ARTICULO")
#ELSE
DO CASE
	CASE TYPE("COMCODPRO")#"U" .AND. !EMPTY(COMCODPRO)
		RETURN SAYGRL(COMCODPRO,"COMS","COMCODPRO","COMS.COMPRA")
	CASE !EMPTY(TELCODPRO)
		RETURN SAYGRL(TELCODPRO,"TELS","TELCODPRO","TELS.TELA")
	CASE TYPE("OTRCODPRO")#"U" .AND. !EMPTY(OTRCODPRO)
		RETURN SAYGRL(OTRCODPRO,"OTRS","OTRCODPRO","OTRS.OTRO")
ENDCASE
RETURN SAYGRL(AVICODPRO,"AVIS","AVICODPRO","AVIS.AVIO")
#ENDIF