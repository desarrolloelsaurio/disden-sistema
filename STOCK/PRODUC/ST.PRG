*       ķ
*                                                                
*        _R260R70C6  ON SELE BAR 1 OF POPU listadopor      
*                                                                
*        Procedure Origin:                                       
*                                                                
*        From Menu:  STOCK.MPR,            Record:  438          
*        Called By:  ON SELE BAR 1 OF POPU listadopor      
*        PROM:     Reimprime C.I.D.                            
*        Snippet:    44                                          
*                                                                
*       Ľ
*
PROCEDURE _r260r70c6
	DIME ARTI[5], DESCR[5], CANTI[5], UNID[5]
	=OPMER()
	=OPCID()
	=OPPRO()
	=OPAVIS()
	=OPTELS()
	=OPCOMS()
	=OPOTRS()
	=OPCOR()
	=OPMIX()
	STORE NUL TO M.RESPUESTA
	FINREQ=GETNU1("C.I.D.","FOXGRL")+1
	IF !EMPTY(INFIC(FINREQ,"N de C.I.D. a Listar"))
		SELE CID
		SET ORDER TO TAG NUMERO
		IF SEEK(M.RESPUESTA)
			SCAT MEMV
			SELE CIX
			SET ORDER TO TAG NUMERO
			IF SEEK(M.NUMERO)
				M.XXZZ=1
				SCAN WHILE NUMERO=M.NUMERO
					ARTI[M.XXZZ]  = ELCOD()
					DESCR[M.XXZZ] = ELTEX()
					CANTI[M.XXZZ] = IIF(EMPTY(ENTRADA), SALIDA, ENTRADA)
					UNID[M.XXZZ]  = ELUNI()
					M.DETALLE=DETALLE
					M.XXZZ=M.XXZZ+1
				ENDSCAN
				DO LCID
			ELSE
				=CARTEL("NO SE HAN ENCONTRADO DATOS DE ARTICULO",;
					"EN EL C.I.D. N "+ALLTRIM(STR(M.NUMERO)),;
					"IMPOSIBLE REIMPRIMIR",;
					ENTERCON)
			ENDIF
		ELSE
			=NOSELEC("N DE C.I.D.")
		ENDIF
	ELSE
		=NOSELEC("N DE C.I.D.")
	ENDIF
RETURN

