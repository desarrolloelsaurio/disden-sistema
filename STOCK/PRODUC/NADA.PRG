USE GETF('MNX')
DIME NIVELES[1], POSICION[1]
STORE 0 TO NIVELACT, LONG
SCAN
	CUAL = ASCAN(NIVELES, LEVELNAME)
	IF EMPTY(CUAL)
		LONG=LONG+1
		DIME NIVELES[LONG], POSICION[LONG]
		NIVELES[LONG]=LEVELNAME
		POSICION[LONG]=NIVELACT+1
	ELSE
		NIVELACT=POSICION[CUAL]
	ENDIF
ENDSCAN
SCAN
	CUAL = ASCAN(NIVELES, LEVELNAME)
	IF !EMPTY(PROMPT)
		?SPACE(POSICION[CUAL]*3)+"|  "+PROMPT
	ENDIF
ENDSCAN
RETURN