FUNCTION ELMON
DO CASE
	CASE !EMPTY(COMCODPRO)
		RETURN SAYGRL(COMCODPRO,"COMS","COMCODPRO","COMS.MONTO")
	CASE !EMPTY(TELCODPRO)
		RETURN SAYGRL(TELCODPRO,"TELS","TELCODPRO","TELS.MONTO")
	CASE !EMPTY(OTRCODPRO)
		RETURN SAYGRL(OTRCODPRO,"OTROS","OTRCODPRO","OTRS.MONTO")
ENDCASE
RETURN SAYGRL(AVICODPRO,"AVIS","AVICODPRO","AVIS.MONTO")
