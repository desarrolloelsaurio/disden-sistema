FUNCTION ELCOD
PARAMETER EL_NUM
#IF "NOVO" $ CURD()
	RETURN IIF(EL_NUM,5,ARTICULO)
#ELSE
DO CASE
CASE !EMPTY(AVICODPRO)
	RETURN IIF(EL_NUM,1,AVICODPRO)
CASE !EMPTY(TELCODPRO)
	RETURN IIF(EL_NUM,2,TELCODPRO)
CASE !EMPTY(COMCODPRO)
	RETURN IIF(EL_NUM,3,COMCODPRO)
ENDCASE
RETURN IIF(EL_NUM,4,OTRCODPRO)
#ENDIF