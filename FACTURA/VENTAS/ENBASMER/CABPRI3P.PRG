FUNCTION CABPRI3P
	parameter copias
	Xa=''
	IF TYPE("ENASCII")#"U" .AND. ENASCII
		XA=ENASCII()
	ENDIF
	SET DEVICE TO PRINT
	SET CONSOLE OFF
	SET PRINTER ON
	IF !EMPTY(XA)
		SET PRINTER TO (XA)
	ENDIF
	_PEJECT="NONE"
	_PLINENO=NUL
	_PAGENO=1
	_PSCODE='{27}{64}'
	DO CASE
	CASE LEFT(JANNAME,5)="PALAD" .OR. LEFT(JANNAME,5)="ARCO " .OR. LEFT(JANNAME,7)="GUIPER "
		_PLENGTH=51
	CASE LEFT(JANNAME,5)="PURAL" .OR. RIGHT(ALLTRIM(JANNAME),5)="MELAC" .OR. LEFT(JANNAME,5)="GUILL" 
		_PLENGTH=48
	CASE LEFT(JANNAME,5)="CLAUD" .OR. LEFT(JANNAME,5)="DEMOS" .OR. LEFT(JANNAME,4)="ALGO"
		_PLENGTH=72
	ENDCASE
	_PLOFFSET=NUL
	_ALIGNMENT='LEFT'
	_PADVANCE="LINEFEEDS"
	_wrap=.F.
	_rmargin=255
	_lmargin=NUL
	_PCOPIES=copias
return .t.
