FUNCTION LFACR3
	? PAIP
	? IIF(FILFAC=3,SUCU+'-'+ZERO(ELARTICULO,8)+PAIP+LAMERCA,IIF(FILFEC=3,ELARTICULO,IIF(FILvia=3 .OR. FILCLI=3,LAFACTURA,"")))
	?? PAIP
	?? STR(CAN,XACDIGI)
	?? PAIP
	TCAN=TCAN+CAN
	STORE NUL TO CAN
	IF FILFEC=3 .or. filvia=3 .OR. FILCLI=3
		?? STR(FAC,XACDIGI)
		?? PAIP
		TFAC=TFAC+FAC
		STORE NUL TO FAC
	ENDIF
	?? STR(TOT,XACDIGI,2)
	?? PAIP
	TTOT=TTOT+TOT
	STORE NUL TO TOT
RETURN LFACR0(.T.)
