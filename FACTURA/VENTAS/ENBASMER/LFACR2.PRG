FUNCTION LFACR2
	can=can+FAX.cantidad
	IF FILFEC=3
		IF FAX.FACTURA#LAFACTURA
			STORE FAC+1 TO FAC
			STORE TOT+FAC.MONTONET TO TOT
			LAFACTURA=FAX.FACTURA
		ENDIF
	ELSE
		IF FILFAC=3
			STORE FAC.MONTONET TO TOT
		ELSE
			IF FILVIA=3
				IF FAX.FACTURA#LAMERCA
					STORE FAC+1 TO FAC
					STORE TOT+FAC.MONTONET TO TOT
					LAMERCA=FAX.FACTURA
				ENDIF
			ELSE
				IF FILCLI=3
					IF FAX.FACTURA#LAMERCA
						STORE FAC+1 TO FAC
						STORE TOT+FAC.MONTONET TO TOT
						LAMERCA=FAX.FACTURA
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	ENDIF
RETURN .T.
