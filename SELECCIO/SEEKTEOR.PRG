FUNCTION SEEKTEC
SET CONF OFF
SET NEAR ON
SET EXACT OFF
SET BELL OFF
USE H:\ARCHIVOS\BALCALA\CLIENTES ORDER TAG CLIENTE
STORE '' TO LEER
@ 2,10 GET LEER
CLEA GETS
FOR A=1 TO 20
LEER1=SUBSTR(CLIENTE,A,1)
@ 2,9+A GET LEER1 PICT 'X'
READ
IF (LAST()>31 .AND. LAST()<123)
LEER=LEER+LEER1
ELSE
IF LAST()#13
LEER=LEFT(LEER,LEN(LEER)-1)
A=A-IIF(A>1,2,0)
ENDIF
ENDIF
=SEEK(LEER)
NADA=CLIENTE
@ 2,10 GET NADA
CLEA GETS
IF LAST()=13 .OR. LAST()=27
EXIT
ENDIF
ENDF
SET CONF ON
SET EXACT ON
SET BELL ON
SET NEAR OFF
RETURN LAST()#27
