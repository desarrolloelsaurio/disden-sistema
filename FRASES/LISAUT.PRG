WAN=WONTOP()
=WIN0()
ALA=ALIAS()
SELE AUT
ORD=ORDER()
SET ORDER TO TAG AUTOR
STORE 0 TO REN
SCAN
@ REN,1 SAY CODIGO
?? " "
?? AUTOR
REN=REN+1
   IF REN=23
      IF INKEY(0,"HM")=27
         EXIT
      ELSE
         CLEA
         STORE 0 TO REN
      ENDIF
   ENDIF
ENDSCAN
IF LAST()#27
=INKEY(0,"HM")
ENDIF
IF !EMPTY(ORD)
SET ORDER TO TAG (ORD)
ENDIF
RELE WIND WIN0
IF !EMPTY(WAN)
ACTI WIND (WAN)
ENDIF
SELE (ALA)
RETURN