WAN=WONTOP()
=WIN0()
ALA=ALIAS()
SELE PER
ORD=ORDER()
STORE 0 TO REN
SCAN
@ REN,1 SAY CODCLI
?? " "
?? CLIENTE
REN=REN+1
   IF REN=23
      IF INKEY(0,"HM")=27
         EXIT
      ELSE
         CLEA
         STORE 0 TO REN
      ENDIF
   ENDIF
ENDSCAN
IF LAST()#27
=INKEY(0,"HM")
ENDIF
IF !EMPTY(ORD)
SET ORDER TO TAG (ORD)
ENDIF
RELE WIND WIN0
IF !EMPTY(WAN)
ACTI WIND (WAN)
ENDIF
SELE (ALA)
RETURN
