WAN=WONTOP()
=WIN0()
ALA=ALIAS()
SELE PER
SET ORDER TO TAG CODCLI
SELE MIX
SET RELA TO TEXTO1 INTO PER ADDI
STORE 0 TO REN
SCAN
@ REN,1 SAY PER.CLIENTE
?? " "
?? TEXTO2
?? " "
?? STR(NU1,4)
REN=REN+1
   IF REN=23
      IF INKEY(0,"HM")=27
         EXIT
      ELSE
         CLEA
         STORE 0 TO REN
      ENDIF
   ENDIF
ENDSCAN
IF LAST()#27
=INKEY(0,"HM")
ENDIF
RELE WIND WIN0
IF !EMPTY(WAN)
ACTI WIND (WAN)
ENDIF
SELE (ALA)
RETURN
