FUNCTION PROXCHEQ
	PARAMETER PRX_COBAN
	#DEFINE NUL    0                              && NUL
	STORE NUL TO PRX_CHEQUE
	IF !EMPTY(PRX_COBAN)
		PRX_ALIAS=ALIAS()
		PRX_ORD1=ORDER()
		SELE CCB
		PRX_ORD2=ORDER()
		SET ORDER TO
		CALC MAX(NROCHEQ) FOR CODBAN=PRX_COBAN .AND. ;
			!EMPTY(FECHAVEN) .AND. ;
			MONTO<0 ;
			TO PRX_CHEQUE
		IF !EMPTY(PRX_ORD2)
			SET ORDER TO TAG (PRX_ORD2)
		ENDIF
		IF !EMPTY(PRX_ALIAS)
			SELE (PRX_ALIAS)
			IF !EMPTY(PRX_ORD1)
				SET ORDER TO TAG (PRX_ORD1)
			ENDIF
		ENDIF
	ELSE
		STORE -1 TO PRX_CHEQUE
	ENDIF
RETURN PRX_CHEQUE+1
