FUNCTION ARMA_FEC
PARA AR__DIA,AR__MES,AR__ANIO,EN_CHAR
IF AR__MES>12
   AR__FEC=ctod(str(AR__DIA,2)+'/12/'+str(AR__ANIO,4))
   AR__FEC=GOMONTH(AR__FEC,AR__MES-12)
ELSE
   AR__FEC=ctod(str(AR__DIA,2)+'/'+str(AR__MES,2)+'/'+str(AR__ANIO,4))
ENDIF
AR__FEC=IIF(EN_CHAR,DTOC(AR__FEC),AR__FEC)
RETU AR__FEC
