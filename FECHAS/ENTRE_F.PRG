PARAMETERS en_tex
DO CASE
CASE _DOS OR _UNIX      && no UNIX records in screen

	
	*       ЦДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДД·
	*       є                                                         є
	*       є         ENTRE_FE/MS-DOS Setup Code - SECTION 1          є
	*       є                                                         є
	*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
	*
	
	#REGION 1
	if _windows
	   STORE IIF(EMPTY(EN_TEX)," LЎmites de Fecha ",EN_TEX) TO EN_TEX
	else
	   STORE IIF(EMPTY(EN_TEX)," Lнmites de Fecha ",EN_TEX) TO EN_TEX
	endif
	
	#REGION 0
	REGIONAL m.currarea, m.talkstat, m.compstat
	
	IF SET("TALK") = "ON"
		SET TALK OFF
		m.talkstat = "ON"
	ELSE
		m.talkstat = "OFF"
	ENDIF
	m.compstat = SET("COMPATIBLE")
	SET COMPATIBLE FOXPLUS
	
	*       ЦДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДД·
	*       є                                                         є
	*       є                MS-DOS Window definitions                є
	*       є                                                         є
	*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
	*
	
	IF NOT WEXIST("entrefe") ;
		OR UPPER(WTITLE("ENTREFE")) == "ENTREFE.PJX" ;
		OR UPPER(WTITLE("ENTREFE")) == "ENTREFE.SCX" ;
		OR UPPER(WTITLE("ENTREFE")) == "ENTREFE.MNX" ;
		OR UPPER(WTITLE("ENTREFE")) == "ENTREFE.PRG" ;
		OR UPPER(WTITLE("ENTREFE")) == "ENTREFE.FRX" ;
		OR UPPER(WTITLE("ENTREFE")) == "ENTREFE.QPR"
		DEFINE WINDOW entrefe ;
			FROM INT((SROW()-6)/2),INT((SCOL()-46)/2) ;
			TO INT((SROW()-6)/2)+5,INT((SCOL()-46)/2)+45 ;
			TITLE " LЎmites de Fecha " ;
			FLOAT ;
			NOCLOSE ;
			MINIMIZE ;
			DOUBLE ;
			COLOR SCHEME 1
	ENDIF
	
	
	*       ЦДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДД·
	*       є                                                         є
	*       є              ENTRE_FE/MS-DOS Screen Layout              є
	*       є                                                         є
	*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
	*
	
	#REGION 1
	IF WVISIBLE("entrefe")
		ACTIVATE WINDOW entrefe SAME
	ELSE
		ACTIVATE WINDOW entrefe NOSHOW
	ENDIF
	@ 1,22 GET M.FECINI ;
		SIZE 1,8 ;
		DEFAULT {  /  /  } ;
		PICTURE "@K" ;
		MESSAGE "Fecha Inicial "+en_tex
	@ 2,22 GET M.FECFIN ;
		SIZE 1,8 ;
		DEFAULT {  /  /  } ;
		PICTURE "@K" ;
		WHEN last()#27 .and. !empty(m.fecini) ;
		MESSAGE "Fecha Final "+en_tex
	@ 1,1 SAY "FECHA INICIAL" ;
		SIZE 1,13, 0
	@ 2,1 SAY "FECHA FINAL" ;
		SIZE 1,11, 0
	
	IF NOT WVISIBLE("entrefe")
		ACTIVATE WINDOW entrefe
	ENDIF
	
	READ
	
	RELEASE WINDOW entrefe
	
	#REGION 0
	IF m.talkstat = "ON"
		SET TALK ON
	ENDIF
	IF m.compstat = "ON"
		SET COMPATIBLE ON
	ENDIF
	
	
	*       ЦДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДД·
	*       є                                                         є
	*       є              ENTRE_FE/MS-DOS Cleanup Code               є
	*       є                                                         є
	*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
	*
	
	#REGION 1
	EN_COSA=lastkey()#27
	EN_COSA=IIF(EN_COSA,!empty(fecini),EN_COSA)
	EN_COSA=IIF(EN_COSA,!empty(fecfin),EN_COSA)
	EN_COSA=IIF(EN_COSA,FECINI<=FECFIN,EN_COSA)
	RETURN EN_COSA
	

CASE _WINDOWS OR _MAC   && no MAC records in screen

	
	*       ЦДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДД·
	*       є                                                         є
	*       є         ENTRE_FE/Windows Setup Code - SECTION 1         є
	*       є                                                         є
	*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
	*
	
	#REGION 1
	STORE IIF(EMPTY(EN_TEX)," Lнmites de Fecha ",EN_TEX) TO EN_TEX
	
	
	#REGION 0
	REGIONAL m.currarea, m.talkstat, m.compstat
	
	IF SET("TALK") = "ON"
		SET TALK OFF
		m.talkstat = "ON"
	ELSE
		m.talkstat = "OFF"
	ENDIF
	m.compstat = SET("COMPATIBLE")
	SET COMPATIBLE FOXPLUS
	
	m.rborder = SET("READBORDER")
	SET READBORDER OFF
	
	*       ЦДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДД·
	*       є                                                         є
	*       є               Windows Window definitions                є
	*       є                                                         є
	*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
	*
	
	IF NOT WEXIST("_rcy0jdb3i")
		DEFINE WINDOW _rcy0jdb3i ;
			AT  0.000, 0.000  ;
			SIZE 6.077,62.400 ;
			TITLE " Lнmites de Fecha " ;
			FONT "MS Sans Serif", 8 ;
			FLOAT ;
			NOCLOSE ;
			MINIMIZE ;
			SYSTEM ;
			COLOR RGB(,,,0,128,0)
		MOVE WINDOW _rcy0jdb3i CENTER
	ENDIF
	
	
	*       ЦДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДД·
	*       є                                                         є
	*       є             ENTRE_FE/Windows Screen Layout              є
	*       є                                                         є
	*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
	*
	
	#REGION 1
	IF WVISIBLE("_rcy0jdb3i")
		ACTIVATE WINDOW _rcy0jdb3i SAME
	ELSE
		ACTIVATE WINDOW _rcy0jdb3i NOSHOW
	ENDIF
	@ 1.308,2.400 SAY "FECHA INICIAL" + CHR(13) + ;
		"FECHA FINAL" ;
		SIZE 2.000,15.300, 0.000 ;
		FONT "Arial", 16 ;
		STYLE "BT" ;
		COLOR RGB(255,255,255,,,,)
	@ 1.308,36.400 GET M.FECINI ;
		SIZE 0.960,10.400 ;
		DEFAULT {  /  /  } ;
		FONT "Arial", 16 ;
		STYLE "BI" ;
		PICTURE "@K" ;
		MESSAGE "Fecha Inicial "+en_tex ;
		COLOR ,RGB(255,255,255,0,128,0)
	@ 3.231,36.400 GET M.FECFIN ;
		SIZE 0.960,10.400 ;
		DEFAULT {  /  /  } ;
		FONT "Arial", 16 ;
		STYLE "BI" ;
		PICTURE "@K" ;
		WHEN last()#27 .and. !empty(m.fecini) ;
		MESSAGE "Fecha Final "+en_tex ;
		COLOR ,RGB(255,255,255,0,128,0)
	
	IF NOT WVISIBLE("_rcy0jdb3i")
		ACTIVATE WINDOW _rcy0jdb3i
	ENDIF
	
	READ
	
	RELEASE WINDOW _rcy0jdb3i
	
	#REGION 0
	
	SET READBORDER &rborder
	
	IF m.talkstat = "ON"
		SET TALK ON
	ENDIF
	IF m.compstat = "ON"
		SET COMPATIBLE ON
	ENDIF
	
	
	*       ЦДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДД·
	*       є                                                         є
	*       є              ENTRE_FE/Windows Cleanup Code              є
	*       є                                                         є
	*       УДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДЅ
	*
	
	#REGION 1
	EN_COSA=lastkey()#27
	EN_COSA=IIF(EN_COSA,!empty(fecini),EN_COSA)
	EN_COSA=IIF(EN_COSA,!empty(fecfin),EN_COSA)
	EN_COSA=IIF(EN_COSA,FECINI<=FECFIN,EN_COSA)
	RETURN EN_COSA
	

ENDCASE
