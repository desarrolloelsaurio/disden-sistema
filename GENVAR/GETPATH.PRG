FUNCTION GETPATH
PARAMETER ELPATH
RELE PATH,PATHPRO,PATHCOM,PATHVEN,PATHCPR,PATHCAJ,PATHTMP
PUBL  PATH,PATHPRO,PATHCOM,PATHVEN,PATHCPR,PATHCAJ,PATHTMP
IF EMPTY(ELPATH)
   ELPATH="PATH"
ENDIF   
ELPATH=ELPATH+".MEM"
C_POS=13
DIME POSIB[C_POS]
POSIB[1]="F:\PUBLIC\"+ELPATH
POSIB[2]="\PUBLIC\"+ELPATH
POSIB[3]="F:\USR\"+GETENV("USERNAME")+"\"+ELPATH
POSIB[4]="PATH.MEM"
POSIB[5]=IIF(TYPE("USADOR")="C","\USR\"+USADOR+"\"+ELPATH,"FOXPSER.DBF")
POSIB[6]="\FOX2\"+ELPATH
POSIB[7]="\FOXPRO2\"+ELPATH
POSIB[8]="C:\FOX2\"+ELPATH
POSIB[9]="C:\FOXPRO2\"+ELPATH
POSIB[10]="F:\FOXPRO2\"+ELPATH
POSIB[11]="F:\FOXPRO2\"+ELPATH
POSIB[12]="G:\FOXPRO2\"+ELPATH
POSIB[13]="G:\FOXPRO2\"+ELPATH
FOR XA= 1 TO C_POS
   IF FILE(POSIB[XA])
      REST FROM (POSIB[XA]) ADDI
      EXIT
   ENDIF
NEXT
RELE POSIB
RETURN .T.
