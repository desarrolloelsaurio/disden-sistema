FUNCTION SAVUNID
ELUND='UNID'
IF GETENV("AUTORIZ")="SI"
   IF EMPTY(GETENV("EQUIPO")) .OR. GETENV("EQUIPO")="DESARROLLO" .OR. GETENV("EQUIPO")="MICASA"
      SAVE TO UNID ALL LIKE UNI*
   ELSE
      FA="F:\PUBLIC\UNID.MEM"
      IF FILE(FA)
         SAVE TO (FA) ALL LIKE UNI*
      ELSE
         FA="F:\USR\"+GETENV("USERNAME")+"\UNID.MEM"
         IF FILE(FA)
            SAVE TO (FA) ALL LIKE UNI*
         ELSE
            SAVE TO (ELUND) ALL LIKE UNI*
         ENDIF
      ENDIF
   ENDIF
ENDIF
RELE ALL LIKE UNI*
RETURN .T.
