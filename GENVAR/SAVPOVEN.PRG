FUNCTION SAVPOVEN
PARAMETER NADA
RELE PATH,pathpro
C_POS=5
DIME POSIB[C_POS]
POSIB[1]="F:\USR\"+IIF(EMPTY(GETENV("USERNAME")),"",GETENV("USERNAME")+"\")+"POSICVEN.MEM"
POSIB[2]="\SISTEMAS\VENTAS\VENTASP\POSIC.MEM"
POSIB[3]="\SISTEMAS\VENTAS\VENTASA\POSIC.MEM"
POSIB[4]=".\POSIC.MEM"
POSIB[5]=NADA+"\POSIC.MEM"
FOR XA=1 TO C_POS
   IF FILE(POSIB[XA])
      SAVE TO (POSIB[XA]) ALL EXCE POSIB*
      EXIT
   ENDIF
NEXT
RETURN .T.
