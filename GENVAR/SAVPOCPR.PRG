POSIB[2]="\SISTEMAS\COMPRAS\COMPRASP\POSIC.MEM"
POSIB[3]="\SISTEMAS\COMPRAS\COMPRASA\POSIC.MEM"
POSIB[4]=".\POSIC.MEM"
POSIB[5]=NADA+"\POSIC.MEM"
FOR XA=1 TO C_POS
