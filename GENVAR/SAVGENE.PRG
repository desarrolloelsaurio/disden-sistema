FUNCTION SAVGENE
IF !EMPTY(GETENV("RESPONS")) .OR. ;
   ESJEFE() .OR. ;
   USADOR="NANO"
   IF FILE("F:\PUBLIC\GENE.MEM")
      SAVE TO F:\PUBLIC\GENE ALL LIKE xa*
   ELSE
      IF FILE("\PUBLIC\GENE.MEM")
         SAVE TO \PUBLIC\GENE ALL LIKE xa*
      ELSE
         IF FILE("\SISTEMAS\VENTAS\VENTASP\GENE.MEM")
            SAVE TO \SISTEMAS\VENTAS\VENTASP\GENE ALL LIKE xa*
         ELSE
            IF FILE("\SISTEMAS\VENTAS\VENTASA\GENE.MEM")
               SAVE TO \SISTEMAS\VENTAS\VENTASA\GENE ALL LIKE xa*
            ELSE
         IF FILE("\SISTEMAS\PRODUC\PRODUCA\GENE.MEM")
               SAVE TO \SISTEMAS\PRODUC\PRODUCA\GENE ALL LIKE xa*
         ELSE
               SAVE TO GENE ALL LIKE xa*
            ENDIF
            ENDIF
         ENDIF
      ENDIF
   ENDIF
ENDIF
RETURN .T.
