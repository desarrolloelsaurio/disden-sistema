FUNCTION SAVPOCAJ
PARAMETER NADA
RELE PATH,pathpro
C_POS=5
DIME POSIB[C_POS]
POSIB[1]="F:\USR\"+GETENV("USERNAME")+"\POSICCAJ.MEM"
POSIB[2]="\SISTEMAS\CAJA\CAJAP\POSIC.MEM"
POSIB[3]="\SISTEMAS\CAJA\CAJAA\POSIC.MEM"
POSIB[4]=".\POSIC.MEM"
POSIB[5]=NADA+"\POSIC.MEM"

FOR XA=1 TO C_POS
   IF FILE(POSIB[XA])
      SAVE TO (POSIB[XA])
      EXIT
   ENDIF
NEXT
RETURN .T.
