FUNCTION GETPATH
IF GETENV("EQUIPO")='ARROYO'
   STORE "C:\SISTEMAS\VENTAS\VENTASA" TO PATH,PATHPRO,PATHVEN,PATHCAJ
ELSE
   IF GETENV("EQUIPO")="MICASA"
      STORE "D:\SISTEMAS\VENTAS\VENTASA" TO PATH,PATHPRO,PATHVEN,PATHCAJ
   ELSE
      STORE "G:\SISTEMAS\VENTAS\VENTASA" TO PATH,PATHPRO,PATHVEN
   	  STORE "G:\SISTEMAS\CAJA\CAJAA"     TO PATHCAJ
   ENDIF
ENDIF
RETURN .T.
