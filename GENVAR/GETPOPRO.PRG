FUNCTION GETPOPRO
C_POS=5
RELE POSIB
DIME POSIB[C_POS]
POSIB[1]="F:\USR\"+IIF(EMPTY(GETENV("USERNAME")),IIF(TYPE("USADOR")="C",ALLTRIM(USADOR),""),GETENV("USERNAME")+"\")+"POSICPRO.MEM"
POSIB[2]="\SISTEMAS\PRODUC\PRODUCP\POSIC.MEM"
POSIB[3]="\SISTEMAS\PRODUC\PRODUCA\POSIC.MEM"
POSIB[4]="POSIC.MEM"
POSIB[5]=IIF(TYPE("PATHPRO")="C",PATHPRO,"")+"\POSIC.MEM"
FOR XA=1 TO C_POS
   IF FILE(POSIB[XA])
      REST FROM (POSIB[XA]) ADDI
      EXIT
   ENDIF
NEXT
RELE POSIB
RETURN .T.
