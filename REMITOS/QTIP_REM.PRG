* V: REMITO COMUN DE VENTA
* I: REMITO DE TRANSFERENCIA ENTRE SUCURSALES
* T: REMITO DE TRANSPORTE A CLIENTES
FUNCTION QTIP_REM
PARAMETER Q_TIP
Q_TIP=IIF(EMPTY(Q_TIP),"V",Q_TIP)
RETURN IIF(Q_TIP="I","INTERNO ENTRE SUCURSALES",;
                     IIF(Q_TIP="T","DE TRANSPORTE A CLIENTES",;
                                   "DE VENTA"))