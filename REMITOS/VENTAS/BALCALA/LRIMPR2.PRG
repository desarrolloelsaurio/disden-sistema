FUNCTION LRIMPR2
	can=can+rex.cantidad
	IF FILFEC=3
		IF REX.REMITO#ELREMITO
			STORE REM+1 TO REM
			STORE TOT+REM.VALORIZ TO TOT
			ELREMITO=REX.REMITO
		ENDIF
	ELSE
		IF FILREM=3
			STORE REM.VALORIZ TO TOT
		ELSE
			IF FILVIA=3
				IF REX.REMITO#LAMERCA
					STORE REM+1 TO REM
					STORE TOT+REM.VALORIZ TO TOT
					LAMERCA=REX.REMITO
				ENDIF
			ENDIF
		ENDIF
	ENDIF
RETURN .T.
