SET EXCL OFF
SET TALK ON
CLEA
VIEJO=PADR("57834",20)
NUEVO=PADR("57607",20)
USE F:\SISTEMAS\PRODUC\PRODUCA\TELSTOCK
SET ORDER TO TAG TELCODPRO
IF SEEK(VIEJO)
WAIT "ENCONTRE TELA "+VIEJO WIND NOWA
REPL TELCODPRO WITH NUEVO
WAIT "BUSCANDO REQUERIMIENTOS "+VIEJO WIND NOWA
USE F:\SISTEMAS\PRODUC\PRODUCA\REQUEDAT
REPL TELCODPRO WITH NUEVO FOR TELCODPRO=VIEJO
WAIT "BUSCANDO EN KARDEX "+VIEJO WIND NOWA
USE F:\SISTEMAS\PRODUC\PRODUCA\KARDESTOCK
*LIST FOR TELCODPRO=VIEJO
REPL TELCODPRO WITH NUEVO FOR TELCODPRO=VIEJO
WAIT "BUSCANDO EN DESPIECE "+VIEJO WIND NOWA
USE F:\SISTEMAS\PRODUC\PRODUCA\DESPIECE
*LIST FOR TELCODPRO=VIEJO
REPL TELCODPRO WITH NUEVO FOR TELCODPRO=VIEJO
WAIT "BUSCANDO EN C.I.D. "+VIEJO WIND NOWA
USE F:\SISTEMAS\STOCK\STOCKA\CIDDAT
*LIST FOR TELCODPRO=VIEJO
REPL TELCODPRO WITH NUEVO FOR TELCODPRO=VIEJO
WAIT "BUSCANDO EN REMITOS "+VIEJO WIND NOWA
USE F:\SISTEMAS\PRODUC\PRODUCA\REMITDAF
*LIST FOR TELCODPRO=VIEJO
REPL TELCODPRO WITH NUEVO FOR TELCODPRO=VIEJO
WAIT "BUSCANDO EN FABRICACION "+VIEJO WIND NOWA
USE F:\SISTEMAS\PRODUC\PRODUCA\ORFABDAT
*LIST FOR AVICODPRO=VIEJO .AND. TIPO="M"
REPL AVICODPRO WITH NUEVO FOR AVICODPRO=VIEJO .AND. TIPO="M"
ELSE
WAIT "NO EXISTE LA TELA"+VIEJO WIND 
ENDIF
CLOSE DATA