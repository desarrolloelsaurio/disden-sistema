FUNCTION LAGRO
	PARAMETER IMPRIME
	IF IMPRIME
		? PAIP
		?? LAFACT1
		?? PAIP
		?? STR(DEBE,XACDIGI,2)
		?? PAIP
		?? STR(HABER,XACDIGI,2)
		?? PAIP
		?? STR(DEBE-HABER,XACDIGI,2)
		?? PAIP
		TDEBE=TDEBE+DEBE
		THABER=THABER+HABER
		CDEBE=CDEBE+DEBE
		CHABER=CHABER+HABER
		CASOS=CASOS+1
	ENDIF
	STORE NUL TO DEBE,HABER
	LAFACTURA=VAL(SUBSTR(ccl.COMPR,7))
	LAFACT1=CCL.COMPR
RETURN .T.
