#DEFINE PAIP   "|"                               && PAIP
FUNCTION LCCLALQ0
EXTERNAL ARRAY TOTAL
? PAIP
IF PORPAN
   ?? LEFT(TEMPORAL.CLIENTE,15)
ELSE
   ?? TEMPORAL.CODCLI
   ?? PAIP
   ?? TEMPORAL.CLIENTE
ENDIF
?? PAIP
?? STR(ALQUILERES,XACDIGI,2)
STORE TOTAL[1]+ALQUILERES TO TOTAL[1]
?? PAIP
?? STR(IMPUESTOS,XACDIGI,2)
STORE TOTAL[2]+IMPUESTOS TO TOTAL[2]
?? PAIP
?? STR(PAGOS,XACDIGI,2)
STORE TOTAL[3]+PAGOS TO TOTAL[3]
?? PAIP
M.SALDO=ALQUILERES+IMPUESTOS-PAGOS
STORE TOTAL[4]+M.SALDO TO TOTAL[4]
?? STR(SALDO,XACDIGI,2)
?? PAIP
RETURN .T.
