EXTERNAL PROCEDURE ZERO.PRG
CLEA
SET TALK OFF
CLOSE ALL
ON ERROR RETURN
CLEA
PALABRA=9
@ 10,1 SAY 'INGRESE' GET PALABRA PICT '99999999'
READ
CLEA
IF PALABRA=14642754 .or. PALABRA=13598543
   DO MANAGER.MPR
   STORE "" TO ELBAR
   DO WHILE .T.
      STORE .T. TO XXSAL
      IF EMPTY(ELBAR)
      ACTI MENU _MSYSMENU       
      ELSE
      ACTI MENU _MSYSMENU PAD (ELBAR)
      ENDIF
      ELBAR=PAD()
      IF !XXSAL
         EXIT
      ENDIF   
   ENDDO
ENDIF
RETURN


