FUNCTION QUECAJA
PARAMETER QCAJA
DO CASE
CASE TYPE("QCAJA")="L"
   QSAL="CAJA GENERAL"
CASE QCAJA="-"
   QSAL="CAJA ADMIN. ROSARIO"
CASE QCAJA=SUCU
   QSAL="CAJA RECAUD. "+ALLTRIM(JANNAME)
CASE QCAJA="0004"   
   QSAL="CAJA RECAUD. ARROYO"
CASE QCAJA="0006"      
   QSAL="CAJA RECAUD. EMPALME"
ENDCASE
RETURN QSAL   
   