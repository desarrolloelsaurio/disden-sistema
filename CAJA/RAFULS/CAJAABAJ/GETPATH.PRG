FUNCTION GETPATH
DO CASE
CASE GETENV("OKDBF")="TRUCHO" 
   STORE "C:\SISTEMAS\ALQUILER\ALQUILEA" TO PATH,PATHPRO,PATHVEN,PATHALQ
   STORE "C:\SISTEMAS\EXPENSAS\EXPENSAA" TO PATHCNS,PATHPRO
CASE GETENV("EQUIPO")="GOYOLAP"
   STORE "C:\SISTEMAS\ALQUILER\ALQUILEA" TO PATH,PATHPRO,PATHVEN,PATHALQ
   STORE "C:\SISTEMAS\EXPENSAS\EXPENSAA" TO PATHCNS,PATHPRO
   STORE "C:\SISTEMAS\CAJA\CAJAA"     TO PATHCAJ
CASE GETENV("EQUIPO")="MICASA"
   STORE "D:\SISTEMAS\EXPENSAS\EXPENSAA" TO PATH,PATHPRO,PATHCNS
   STORE "D:\SISTEMAS\ALQUILER\ALQUILEA" TO PATHALQ
   STORE "D:\SISTEMAS\CAJA\CAJAA"     TO PATHCAJ
