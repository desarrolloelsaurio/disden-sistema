FUNCTION QW_PRC
PARAMETER REN,COL,CUAL
PRIVATE REN,COL,CUAL
STORE NUL TO PRC_SALIDA
IF FILPRC=2
   STORE "" TO TT,FISC
   QWSALIDA=QWPRC(MIXGRL("Seleccion de Procesos ","Proceso","Procesos",.t.),cual)
   IF QWSALIDA#".F."
      QW=AT("ú",QWSALIDA)
      QUEPRC=LEFT(QWSALIDA,QW-1)
      TTPRC=SUBSTR(QWSALIDA,QW+1)
      @ REN,COL say LEFT(PRC.PROCFAB,20)
   ELSE
      STORE 1 TO FILPRC
   endif
endif
RETURN PRC_SALIDA#2
