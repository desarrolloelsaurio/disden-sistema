FUNCTION ELIUSU
PARA MSG,R1,C1
SELE PER
IF USUPOP
   SET ORDER TO TAG USUARIO
   ACTI POPUP PERPOP
   STOR !EMPTY(RESPUESTA) TO RESPUESTA
ELSE
   RESPUESTA=ELIMIX1('Usuario '+MSG,'PER','USUCOD',10)
ENDIF
RETURN IIF(LAST()=27 .OR. READKEY()=12,.F.,RESPUESTA)
