FUNCTION ELIINQ
PARA MSG,R1,C1
SELE PER
IF INQPOP
   SET ORDER TO TAG INQUILIN
   ACTI POPUP PERPOP
   STOR !EMPTY(RESPUESTA) TO RESPUESTA
ELSE
*   RESPUESTA=ELIMIX1('Inquilino '+MSG,'PER','INQCOD',10,"I")
RESPUESTA=ELIPER1('Inquilino '+MSG,'PER','INQCOD',10,"I")
ENDIF
RETURN IIF(LAST()=27 .OR. READKEY()=12,.F.,RESPUESTA)
