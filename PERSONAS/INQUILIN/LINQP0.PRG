FUNCTION LINQP0
	SET ORDER TO TAG CIUCOD IN MIX
	SELE PER
	SET RELA TO LOCALIDAD INTO MIX
	IF FISC==".T."
		SET ORDER TO TAG IIF(TYPE("ORDINQ")#'L' .OR. ORDINQ,'CODCLI','CLIENTE')
	ELSE
		IF TYPE("ORDINQ")#'L' .OR. ORDINQ
			INDE ON CODCLI TO (GENTMP()) FOR EVAL(M.FISC) COMP
		ELSE
			INDE ON CLIENTE TO (GENTMP()) FOR EVAL(M.FISC) COMP
		ENDIF
	ENDIF
	acla=HAZ_ACLA()
	linea=REPL("=",LEN(acla))
	TT="LISTADO DE INQUILINOS"+CHR(13)+CHR(10)+TTU
RETURN .T.
