FUNCTION ELICLI
PARA MSG,R1,C1
SELE PER
IF TYPE("XPENSAS")#"U" .AND. XPENSAS
   IF CSSPOP
      SET ORDER TO TAG CONSORCI
      ACTI POPUP PERPOP
      STOR !EMPTY(RESPUESTA) TO RESPUESTA
   ELSE
      RESPUESTA=ELIMIX1('Consorcista '+MSG,'PER','CSSCOD',10)
   ENDIF
ELSE
   IF INQPOP
      SET ORDER TO TAG INQUILIN
      ACTI POPUP PERPOP
      STOR !EMPTY(RESPUESTA) TO RESPUESTA
   ELSE
      RESPUESTA=ELIPER1('Inquilino '+MSG,'PER','INQCOD',10,"I")
   ENDIF
ENDIF
IF USED('PER') .AND. RESPUESTA
   GO RECNO("PER") IN PER
ENDIF
RETURN RESPUESTA
