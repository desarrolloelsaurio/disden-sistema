FUNCTION ELIPLE
PARA MSG,R1,C1
SELE PER
IF INQPOP
   SET ORDER TO TAG EMPLEADO
   ACTI POPUP PERPOP
   STOR !EMPTY(RESPUESTA) TO RESPUESTA
ELSE
RESPUESTA=ELIPER1('Empleado '+MSG,'PER','PLECOD',10,"9")
ENDIF
RETURN IIF(LAST()=27 .OR. READKEY()=12,.F.,RESPUESTA)
