FUNCTION ELICLI
PARA MSG,R1,C1
SELE PER
IF CSSPOP
   SET ORDER TO TAG CONSORCI
   ACTI POPUP PERPOP
   STOR !EMPTY(RESPUESTA) TO RESPUESTA
ELSE
   RESPUESTA=ELIMIX1('Consorcista '+MSG,'PER','CSSCOD',10)
ENDIF
IF USED('PER') .AND. RESPUESTA
   GO RECNO("PER") IN PER
ENDIF
RETURN IIF(LAST()=27 .OR. READKEY()=12,.F.,RESPUESTA)
