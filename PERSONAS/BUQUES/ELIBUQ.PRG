FUNCTION ELIBUQ
PARA MSG,R1,C1
SELE PER
IF BUQPOP
   SET ORDER TO TAG BUQUE
   ACTI POPUP PERPOP
   STOR !EMPTY(RESPUESTA) TO RESPUESTA
ELSE
   RESPUESTA=ELIPER1('Buque '+MSG,'PER','BUQCOD',10,"B")
ENDIF
RETURN IIF(LAST()=27 .OR. READKEY()=12,.F.,RESPUESTA)
