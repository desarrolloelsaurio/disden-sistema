FUNCTION ELIPRP
PARA MSG,R1,C1
SELE PER
IF PRPPOP
   SET ORDER TO TAG PROPIETA
   ACTI POPUP PERPOP
   STOR !EMPTY(RESPUESTA) TO RESPUESTA
ELSE
   RESPUESTA=ELIPER1('Propietario '+MSG,'PER','PRPCOD',10,"P")
ENDIF
RETURN IIF(LAST()=27 .OR. READKEY()=12,.F.,RESPUESTA)
