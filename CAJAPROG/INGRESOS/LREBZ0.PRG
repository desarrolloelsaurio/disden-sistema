FUNCTION LREBZ0
PARAMETER TT1
#DEFINE CRLF CHR(13)+CHR(10)                     && CRLF
    ** GENERA LA CABECERA DEL LISTADO
    ** SEGUN LA CAJA
	=WORKING()
	=DOTALK()
	dime ttl[CANT_MAX],ctrl[CANT_MAX],TEX[CANT_MAX]
	hay=.T.
	=TTLCHEQ(5)
	SELE CAJ
	SET ORDER TO
	ESCHAR=(TYPE("CAJ.NUMERE")="C")
	set order to tag CODCLI in PER
	SELE CAJ
	TMP=GENTMP()
	IF '.T.'=FISC
   	   SORT ON NUMERE TO (TMP) FOR COMPRE='RE'
	ELSE
		FISC=FISC+".AND. COMPRE='RE'"
		SORT ON NUMERE TO (TMP) FOR EVAL(FISC)
	ENDIF
	SELE CAJ
	USE (TMP) ALIAS CAJ EXCL
	set relation to CAJ.CODCLI INTO PER
	acla=HAZ_ACLA()
	linea=REPL("=",LEN(acla))
	tt='Listado de Recibos'+IIF(!EMPTY(TTQUE),TTQUE,"")+;
	CRLF+;
	TT1

	STORE NUL TO CASOS
	=UNTALK()
	=UNWORK()
** FIN CABECERA LISTADO
RETURN IIF(PORPAN,LrebPZ(),LrebIZ())
