FUNCTION PUTNU3
	PARAMETER P_TEXTO,P_CONT
	P_BASANT=ALIAS()
	SELE MIX
	IF XSEEK(P_TEXTO,"MIX","FOXGRL")
		SCAT MEMV
		M.NU3=P_CONT
		=AGRABAR1("MIX",.F.,.T.)
	ELSE
		SCAT MEMV BLAN
		M.TEXTO1=P_TEXTO
		M.NU3=P_CONT
		M.FILTRO="ð"
		=AGRABAR("MIX",.F.,.F.,.T.)
	ENDIF
	IF !EMPTY(P_BASANT)
		SELE (P_BASANT)
	ENDIF
RETURN .T.
