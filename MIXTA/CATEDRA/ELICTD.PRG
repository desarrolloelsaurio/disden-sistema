FUNCTION ELICTD
PARA MSG,R1,C1
SELE MIX
IF CTDPOP
   SET ORDER TO TAG CTDTEX
   ACTI POPUP MIXPOP
   STOR !EMPTY(RESPUESTA) TO RESPUESTA
ELSE
   RESPUESTA=ELIMIX1('Cátedra '+msg,'MIX','CTDCOD')
ENDIF
RETURN IIF(LAST()=27,.F.,RESPUESTA)
