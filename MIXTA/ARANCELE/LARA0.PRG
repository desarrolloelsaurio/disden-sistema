FUNCTION LARA0
#DEFINE NUL    0                                 && NUL
#DEFINE PAIP   "|"                               && PAIP
	=WORKING()
	=DOTALK()
	SELE MIX
	SET ORDER TO
	TMP=GENTMP()
	=FLOCK("MIX") .OR. FLOK("MIX")
	SORT ON TEXTO1,TEXTO2 TO ;
		(TMP) FOR &FISC
	UNLO IN MIX
	=GETDBF(TMP,.F.,"TMP",25,.T.)
	IF TYPE("ORDARA")#"U" .AND. ORDARA
		INDEX ON SUBSTR(TEXTO1,2)+SUBSTR(TEXTO2,2) TAG SHIT
	ENDIF
	STOR NUL TO CASOS
	ACLA=PAIP+;
		PADC("TIPO DE ARANCEL",20)+PAIP+;
		PADC("UNIDAD",20)+PAIP+;
		PADC("MONTO",XACDIGI)+PAIP+;
		"FECHA IN|  USUARIO |"
	M.LINEA=REPL("=",LEN(ACLA))
	TT=TTQUE
	SELE TMP
	=UNTALK()
	=UNWORK()
RETURN .T.
