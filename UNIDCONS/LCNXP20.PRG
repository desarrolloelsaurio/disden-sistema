FUNCTION LCNXP20
	#DEFINE NUL    0                               && NUL
	#DEFINE PAIP   "|"                             && PAIP
	=WORKING()
	STORE nul TO casos
	acla=paip+PADC("CONSORCIO",20)+paip+PADC("UNIDAD",10)+'|TIP|PIS|DTO|FECHA IN|  USUARIO |'
	linea=REPL("=",LEN(acla))
	tt=ttu
	fisc=fsc
	TMP=GENTMP()
	SELE CNX
	=FLOCK() .OR. FLOK()
	SET ORDER TO
	SET ORDER TO TAG CONCOD IN CNS
	SET RELA TO CNX.CONCOD INTO CNS
	SORT ON CNS.CONSORCIO,DENOM ;
	FIELDS CNS.CONSORCIO,DENOM,TIPO,PISO,DTO,FECHA,USUARIO ;
	TO (TMP) ;
	FOR &FISC
		USE
		USE (TMP) ALIAS CNX EXCL
	RETURN UNWORK()
