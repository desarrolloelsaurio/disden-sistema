FUNCTION LCNXP10
   =WORKING()
   SET ORDER TO TAG CSSCOD IN PER
   STORE nul TO casos,TTL
   acla=paip+PADC("UNIDAD",10)+'|TIP|PIS|DTO|'+PADC("OCUPANTE",30)+paip+' PORCEN '+paip
   LINEA=REPL("=",LEN(ACLA))
   tt=ttu
   FISC=FSC
   TMP=GENTMP()
   SELE CNX
   SET ORDER TO
   =FLOCK() .OR. FLOK()
   SORT ON DENOM ;
   TO (TMP) ;
   FOR &FISC
   UNLO IN CNX
   USE
   USE (TMP) ALIAS CNX EXCL
RETURN UNWORK()
