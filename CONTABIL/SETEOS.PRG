FUNCTION SETEOS
CLEAR MACROS
RELE ALL
CLOSE ALL
CLEA ALL
SET DOHI OFF
SET ODOM TO 1
SET EXCL OFF
SET STAT OFF
SET ESCA OFF
SET TALK OFF
SET REPR TO 5
SET DEVE ON
SET MEMO TO 70
SET DECIMALS TO 4
SET LOGE OFF
SET SYSMENU OFF
SET SAFE OFF
SET OPTI ON
SET DELE ON
SET EXCL OFF
SET CONFIRM ON
SET MULTI ON
SET DATE TO FRENCH
SET BELL TO 100,1
SET AUTOSAVE ON
SET MOUSE ON
SET SHAD ON
IF EMPTY(GETENV("LAPTOP"))
   RELE M.NUMLOCKANT, M.CAPLOCKANT
   PUBL M.NUMLOCKANT, M.CAPLOCKANT
   M.NUMLOCKANT=NUMLOCK(.T.)
   M.CAPLOCKANT=CAPSLOCK(.T.)
ENDIF
IF GETENV("EQUIPO")#"MICASA"
   IF GETENV("USERNAME")#"GOYO" .AND. GETENV("USERNAME")#"BATA"
      ON ERROR DO ERROR
   ELSE
      ON ERROR
      SET ESCA ON
      SET DEVE ON
   ENDIF
ELSE
   ON ERROR
   SET ESCA ON
ENDIF
RETURN .T.
