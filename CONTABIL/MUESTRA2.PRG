store RECNO()  to ANTERIOR
store CODIGO   to MCODIGO
store FECHA    to MFECHA
store CONCEPTO to MCONCEPTO
store DEBE     to MDEBE
store HABER    to MHABER
SELE BAL
seek(MCODIGO)
@3,9 say NOMBRE PICT "@X"
@4,9 say EVAL(DEB)-EVAL(HAB) PICT PICU1
SELE MOV
@6,31 say MOVIMIENTO PICT "#,###"
@7,31 say ASIENTO PICT "#,###"
@8,31 say MCODIGO PICT PICU2
@9,31 say MFECHA PICT PICFEC
@10,31 say MCONCEPTO PICT "@X"
@11,31 say MDEBE PICT PICU1
@12,31 say MHABER PICT PICU1
seek(str(KPERIODO,2)+str(NROASI,4))
sum DEBE,HABER to TDEBE,THABER while ASIENTO=NROASI
set order to 0
goto ANTERIOR
set order to 1
@21,15 say TDEBE PICT PICU1
@21,42 say THABER PICT PICU1
=MEN_LIN(SYSNOMBRE)
return
