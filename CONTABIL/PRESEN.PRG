@ 24,0 SAY PADC("SISTEMA DE CONTABILIDAD",80)
RETURN

FUNCTION PRESEN1
FOR A=1 TO 22 STEP 2
   @ A,1 SAY LEFT(B,78) COLO SCHE CSH
