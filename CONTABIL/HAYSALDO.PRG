FUNCTION HAYSALDO
PARAMETER H_MES2,H_BALANCE
IF H_BALANCE .AND. (!EMPTY(DEBE0) .OR. !EMPTY(HABER0))
   RETURN .T.
ENDIF   
SCAT TO ACNADA
FOR PER=KPERINIC TO IIF(H_MES2<KPERINIC,H_MES2+12,H_MES2)
	PER2=IIF(PER>12,PER-12,PER)
	POSIC=(PER2+2)*2
    IF !EMPTY(ACNADA[POSIC]) .OR. !EMPTY(ACNADA[POSIC+1])
       RETURN .T.
    ENDIF
NEXT
RELE ACNADA
RETURN .F.
