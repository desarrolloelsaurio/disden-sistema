FUNCTION SUMPER
	STORE 0 TO MSPAR
	SCAT TO NADA
	IF LEFT(AP,1)="P"
	   POSIC=(PERINI+2)*2
	   STORE NADA[POSIC]-NADA[POSIC+1] TO MSPAR
	ELSE
	   IF ARRASANIO
		  STORE DEBE0-HABER0 TO MSPAR
	    ENDIF
		MI=PERINI
		MF=IIF(PERFIN<PERINI,PERFIN+12,PERFIN)
		FOR A=PERINI TO MF
			XX=IIF(A>12,A-12,A)
			POSIC=(XX+2)*2
			STORE NADA[POSIC]-NADA[POSIC+1]+MSPAR TO MSPAR
		NEXT
	ENDIF
RETURN MSPAR
