FUNCTION SGET
PARAMETER SHOW_VEN,S_TIPO,SHOW_ACTI
IF PARAMETER()<2
   IF !EMPTY(SHOW_VEN)
      SHOW GETS WIND (SHOW_VEN)
   ENDIF
ELSE
   IF EMPTY(SHOW_ACTI)
      SHOW GETS WIND (SHOW_VEN) OFF
   ELSE
      IF !WONTOP((SHOW_VEN))
         ACTI WIND (SHOW_VEN) 
      ENDIF  
      SHOW GETS WIND (SHOW_VEN)   
   ENDIF
ENDIF
RETURN .T.