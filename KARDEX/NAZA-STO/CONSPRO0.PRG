FUNCTION CONSPRO0
	#DEFINE PAIP   "|"                            && PAIP
	PARA SOLOSALIDA
	IF !EMPTY(SALIDA)
		?paip
		??LEFT(CLIENTE,20)
		??PAIP
		IF !SOLOSALIDA
			FOR XA=1 TO 12
				AA=EVAL("E"+ALLTR(STR(XA,2)))
				??STR(AA,XACDIGI)
				??PAIP
			NEXT
			??STR(SALIDA,XACDIGI+2)
		ELSE
			??SALIDA PICT "9,999,999,999.99"
		ENDIF
		??PAIP
	ENDIF
RETURN .T.
