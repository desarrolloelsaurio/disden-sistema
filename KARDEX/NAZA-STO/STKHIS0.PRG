#DEFINE PAIP   "|"                               && PAIP
FUNCTION STKHIS0
	M.ELST=M.ENT-M.SAL
	IF M.ELST>0
		M.MON=M.ELST*MON_CTRL
		? PAIP
		?? COD_CTRL
		?? PAIP
		?? LEFT(TEX_CTRL,IIF(PORPAN,20,30))
		?? PAIP
		?? STR(M.ELST,XACDIGI)
		?? PAIP
		?? LEFT(UNI_CTRL,2)
		?? PAIP
		?? STR(MON_CTRL,XACDIGI,4)
