FUNCTION QMOV
PARAMETER Q_COD
DO CASE
CASE TYPE("Q_COD")="N"
   DO CASE
   CASE Q_COD=1
      RETURN "AS"
   CASE Q_COD=2
      RETURN "TC"
   CASE Q_COD=3
      RETURN "PX"
   CASE Q_COD=4
      RETURN "DC"
   CASE Q_COD=5
      RETURN "DT"
   CASE Q_COD=6
      RETURN "RS"
   CASE Q_COD=7
      RETURN "AX"
   CASE Q_COD=8
      RETURN "AD"
   CASE Q_COD=9
      RETURN "DP"
   CASE Q_COD=10
      RETURN "OP"
   CASE Q_COD=11
      RETURN "AR"
   ENDCASE
CASE TYPE("Q_COD")="C"
   IF LEN(Q_COD)>2
      DO CASE
      CASE Q_COD="AJUSTE DE STOCK"
         RETURN "AS"
      CASE Q_COD="TELAS AL CORTE"
         RETURN "TC"
      CASE Q_COD="PROTOTIPO"
         RETURN "PX"
      CASE Q_COD="DEVOLUCION DE RESTOS"
         RETURN "DC"
      CASE Q_COD="DEVOLUCION DE TALLERES"
         RETURN "DT"
      CASE Q_COD="RESERVA"
         RETURN "RS"
      CASE Q_COD="AVIOS PARA PROTOTIPO"
         RETURN "AX"
      CASE Q_COD="AVIOS A DISE¥O"
         RETURN "AD"
      CASE Q_COD="DEVOLUCION PROD. TERM."
         RETURN "DP"
      CASE Q_COD="ENTREGA DE OTROS PROD."
         RETURN "OP"
      CASE Q_COD="ANULACION DE RESERVA"
         RETURN "AR"
      ENDCASE
   ELSE
      DO CASE
      CASE Q_COD="AS"
         RETURN "AJUSTE DE STOCK"
      CASE Q_COD="TC"
         RETURN "TELAS AL CORTE"
      CASE Q_COD="PX"
         RETURN "PROTOTIPO"
      CASE Q_COD="DC"
         RETURN "DEVOLUCION DE RESTOS"
      CASE Q_COD="DT"
         RETURN "DEVOLUCION DE TALLERES"
      CASE Q_COD="RS"
         RETURN "RESERVA"
      CASE Q_COD="AX"
         RETURN "AVIOS PARA PROTOTIPO"
      CASE Q_COD="AD"
         RETURN "AVIOS A DISE¥O"
      CASE Q_COD="DP"
         RETURN "DEVOLUCION PROD. TERM."
      CASE Q_COD="OP"
         RETURN "ENTREGA DE OTROS PROD."
      CASE Q_COD="AR"
         RETURN "ANULACION DE RESERVA"
      ENDCASE
   ENDIF
ENDCASE
RETURN
