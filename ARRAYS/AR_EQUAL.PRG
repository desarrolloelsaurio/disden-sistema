FUNCTION AR_EQUAL
PARA AR_ARRA1,AR_ARRA2
PRIV AR_ARRA1,AR_ARRA2,AR_SALIDA,AR_LARGO,AR_XA
AR_SALIDA=.T.
IF ALEN(AR_ARRA1,1)=1
   AR_LARGO=MIN(ALEN(AR_ARRA1,2),ALEN(AR_ARRA2,2))
ELSE
   AR_LARGO=MIN(ALEN(AR_ARRA1,1),ALEN(AR_ARRA2,1))
ENDIF
FOR AR_XA=1 TO AR_LARGO
   IF AR_ARRA1[AR_XA]#AR_ARRA2[AR_XA]
      AR_SALIDA=.F.
      EXIT
   ENDIF
next
RETURN AR_SALIDA
