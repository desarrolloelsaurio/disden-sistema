FUNCTION ISALQOK
*DEVUELVE .T. SI EL ALQUILER SE PUEDE GENERAR
STORE ARMA_FEC(ALQ.DIAVENC,MONTH(DATE()),YEAR(DATE())%100) TO M.IS_VENC
STORE ALQ.MONEDA TO M.IS_MONEDA
IS_INDICE=GEN_IND(ALQ.FECHAALQ,M.IS_VENC,ALQ.CODACT)
IF EMPTY(INM.CODINM)
   RETURN .F.
ENDIF
IF EMPTY(ALQ.CODCLI)
   RETURN .F.
ENDIF
IF !ALQ.EMITE
   RETURN .F.
ENDIF
IF ALQ.FECHAVENC<M.IS_VENC .AND. ;
   !SIGUEALQ(ALQ.CODCLI,INM.CODINM)
	RETURN .F.
ENDIF
IF EMPTY(IS_INDICE)
   RETURN .F.
ENDIF
IF EMPTY(ALQ.MONTOBASE)
   RETURN .F.
ENDIF
IF ALQ.MONEDA=LEFT(XADOLS,1)
   IF EMPTY(XACOTIZ)
      RETURN .F.
   ENDIF
ENDIF
RETURN .T.
