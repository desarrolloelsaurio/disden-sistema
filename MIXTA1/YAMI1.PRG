FUNCTION YAMI1
PARAMETER Y_TIT,BLANCO,Y_WIN
IF !EMPTY(Y_TIT)
WAIT Y_TIT+"EXISTE" WIND NOWA
ENDIF
SELE MI1
IF BLANCO
   SCAT MEMO MEMV BLAN
   SCAT MEMO TO RESPALDO BLAN   
   STORE .T. TO INGRESO
ELSE
   SCAT MEMO MEMV
   SCAT MEMO TO RESPALDO
   FF=RECNO()
   REGISTRO=STR(FF)
   UNLO IN MI1
   =LOCK("MI1") .OR. LOK("MI1")
  M.TRANSA=M.TEXTO2
  M.TRANSA1=M.TEXTO1
  STORE .F. TO INGRESO
ENDIF
=SGET("MI1")
IF !EMPTY(Y_TIT)
WAIT CLEA
ENDIF
RETURN .T.
