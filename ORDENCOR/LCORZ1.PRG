FUNCTION LCORZ1
	#DEFINE PAIP   "|"                             && PAIP
	? PAIP
	=IIF(VARIABLE[1],QOUT(STR(NUMERO)+PAIP),"")
	=IIF(VARIABLE[2],QOUT(DTOC(FECHAINI)+PAIP),"")
	=IIF(VARIABLE[3],QOUT(LEFT(MER.ARTICULO,10)+"-"+LEFT(MER.MERCA,29)+PAIP),"")
	=IIF(VARIABLE[4],QOUT(SAYMIX(ESTRUC,"TSTCOD",20)+PAIP),"")
	=IIF(VARIABLE[5],QOUT(DTOC(FECHAFAB)+PAIP),"")
	=IIF(VARIABLE[6],QOUT(DTOC(FECHAFIN)+PAIP),"")
	=IIF(VARIABLE[7],QOUT(LEFT(NRODOCREF,TEXTO[6,2])+PAIP),"")
	=IIF(VARIABLE[8],QOUT(ORDENADO+PAIP),"")
	CASOS=CASOS+1
RETURN .T.
