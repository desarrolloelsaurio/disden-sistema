@ 5,1 SAY 'FINAL. PREVISTA'
@ 6,1 SAY 'FECHA FINALIZACION'
@ 7,1 SAY 'DOCUMENTO'
@ 8,1 SAY 'RESPONSABLE'
@ 9,1 SAY 'PROCESO'
@ 10,1 SAY 'FECHA'
@ 11,1 SAY 'ESTRUCTURA'
IF XSA
@ 1,COL GET M.NUMERO
@ 2,COL GET M.FECHAINI
SET EXAC OFF
=XSEEK(M.ARTICULO,'MER','ARTICULO')
SET EXAC ON
NADA=LEFT(MER.ARTICULO,10)+"-"+ALLTRIM(MER.MERCA)
