FUNCTION MCO1
PARAMETER COSA
CLEA
@ 1,1 SAY 'NUMERO' COLO SCHE 3
@ 2,1 SAY 'COLOR' COLO SCHE 3
@ 3,1 SAY 'TALLE' COLO SCHE 3
@ 4,1 SAY 'CANTIDAD' COLO SCHE 3
IF COSA
   @ 1,COL GET M.NUMERO
   =XSEEK(M.COLOR,'SUE','SUECOD')
   M.NADA=MIX.TEXTO1
   @ 2,COL GET M.NADA
   @ 3,COL GET M.TALLE
   @ 4,COL GET M.CANTINI
   CLEA GETS
ENDIF
RETURN .T.
