* ศออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออออผ
FUNCTION LINM1
	#DEFINE PAIP "|"                               && PAIP
	? PAIP
	=IIF(VARIABLE[1],QOUT(TMP.CODINM+PAIP),"")
	IF TIPOLIS=1
		=IIF(VARIABLE[2],QOUT(IIF(XSEEK(TMP.TIPOUNID,"MIX","TIPCOD"),LEFT(MIX.TEXTO1,TEXTO[2,2]),SPACE(TEXTO[2,2]))+PAIP), "")
		=IIF(VARIABLE[3],QOUT(TMP.DOMICILIO+PAIP),"")
		=IIF(VARIABLE[4],QOUT(SAYMIX(TMP.LOCALIDAD,"CIUCOD",TEXTO[4,2])+PAIP),"")
		=IIF(VARIABLE[5],QOUT(LEFT(PER.CLIENTE,TEXTO[5,2])+PAIP),"")
		=IIF(VARIABLE[6],QOUT(DTOC(TMP.FECHAING)+PAIP),"")
		=IIF(VARIABLE[7],QOUT(IIF(XSEEK(TMP.ESTADO,"MIX","ESTCOD"),LEFT(MIX.TEXTO1,TEXTO[7,2]),SPACE(TEXTO[2,2]))+PAIP),"")
	ELSE
		=IIF(VARIABLE[2],QOUT(TMP.DOMICILIO+PAIP),"")
		=IIF(VARIABLE[3],QOUT(SAYMIX(TMP.LOCALIDAD,"CIUCOD",TEXTO[4,2])+PAIP),"")
		IF TIPOLIS=2
			=IIF(VARIABLE[4],QOUT(left(TMP.MUNIC,texto[4,2])+PAIP),"")
			=IIF(VARIABLE[5],QOUT(left(TMP.MUNICIPAL,texto[5,2])+PAIP),"")
			=IIF(VARIABLE[6],QOUT(XXTOCHAR(TMP.RECINM)+PAIP),"")
			=IIF(VARIABLE[7],QOUT(STR(TMP.PORMUN,texto[7,2],2)+PAIP),"")
			=IIF(VARIABLE[8],QOUT(left(TMP.INMOC,texto[8,2])+PAIP),"")
			=IIF(VARIABLE[9],QOUT(left(TMP.INMOBILIAR,texto[9,2])+PAIP),"")
			=IIF(VARIABLE[10],QOUT(XXTOCHAR(TMP.RECINM)+PAIP),"")
			=IIF(VARIABLE[11],QOUT(STR(TMP.PORINM,texto[11,2],2)+PAIP),"")
			=IIF(VARIABLE[12],QOUT(left(TMP.DIPOC,texto[12,2])+PAIP),"")
			=IIF(VARIABLE[13],QOUT(left(TMP.DIPOS,texto[13,2])+PAIP),"")
			=IIF(VARIABLE[14],QOUT(XXTOCHAR(TMP.RECINM)+PAIP),"")
			=IIF(VARIABLE[15],QOUT(STR(TMP.PORDIP,texto[15,2],2)+PAIP),"")
		ENDIF
	ENDIF
	CASOS=CASOS+1
RETURN .T.
