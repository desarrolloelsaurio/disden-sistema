FUNCTION CARINM
PARAMETER C_VENT
   SELE INM
   SCAT MEMV MEMO BLAN
   SCAT TO RESPALDO MEMO BLAN
   STORE M.CODINM TO M.CODD
   STORE .T. TO INGRESO
   IF XALQ
      SELE ALQ
      SCAT MEMV MEMO BLAN
      SCAT TO RESPALDO MEMO BLAN
      STORE M.CODD TO M.CODINM
   ENDIF
RETURN SGET("INM") .AND. ;
IIF(XINM,SGET("XINM",.F.,!XXINM),.T.) .AND. ;
IIF(XXINM,SGET("XXINM",.F.,!XXXINM),.T.) .AND. ;
IIF(XXXINM,SGET("XXXINM",.F.,.T.),.T.)
