FUNCTION LINMNCOP
ULPR=PROGRAM()
IF CHEQSEG()
=WIN0()
PAG=1
=CABPAN()
=TIT_LIN(1)
SELE INM
SCAN
IF ROW()>21
IF !FIN_LIN(1,NUL)
				EXIT
			ENDIF
			CLEA
			STOR PAG+1 TO PAG
			=CABPAN()
			=TIT_LIN(2)
		ENDIF
		=LINMNCO0()
	ENDSCAN
	=FIN_LIN(2,CASOS)
	=RELEWIN("win")
	=IAUD("")
ELSE
=IAUDI()
ENDIF
RETURN